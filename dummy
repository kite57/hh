                function printdiv(ChartName) {
                    var youthid = document.getElementById("<%= hfYStartdate.ClientID %>").value;
                    var YouthCaseNo = document.getElementById("<%= lblCaseNumberDashboard.ClientID %>").innerHTML;
                    var YouthName = document.getElementById("<%= lblNameDashboard.ClientID %>").innerHTML;
                    if (ChartName == 'Family Connections') {
                        window.open('/Print/FamilyConnections_Print.aspx?YId=' + youthid + '&CaseNo=' + YouthCaseNo + '&Name=' + YouthName, 'newwindow', 'toolbar=no,location=no,menubar=no,width=750,height=500,resizable=no,scrollbars=yes,top=200,left=250');
                        return false;
                    }
                    else {
                        window.open('/Print/YouthConnections_Print.aspx?YId=' + youthid + '&CaseNo=' + YouthCaseNo + '&Name=' + YouthName, 'newwindow', 'toolbar=no,location=no,menubar=no,width=750,height=500,resizable=no,scrollbars=yes,top=200,left=250');
                        return false;
                    }
                }
                function pageLoad() {
                    initSession();
                    setPOpupWidthBasedonScreen();
                    setContentPageHeight();
                    SetMaskEditorPosition();
                    var youthId = '<%= Request.QueryString["YID"] %>';
                    GetKeys(youthId);
                    CheckDateSigned();
                    //School Details Visibility change in Youth Referral Form

                    SchoolDetailsPgLoad();
                    var hearing_impairment = document.getElementById('<%=cbxYouthHearingImpairment.ClientID %>');
                    var seeing_impairment = document.getElementById('<%=cbxYouthSeeingImpairment.ClientID %>');
                    var accomodations = document.getElementById('<%=txtYouthImpairment.ClientID %>');
                    if (!hearing_impairment.checked && !seeing_impairment.checked) {
                        accomodations.disabled = true;
                    }
                    var rating = ["Missing or Not Given", "Significant need for more support", "Need for support", "Strength", "Significant Strength"];
                    //BaseLine
                    var rBFriends = $("#<%=ratBFriends.ClientID %> > a").attr("title");
                    var rBSchwork = $("#<%=ratBSchwork.ClientID %> > a").attr("title");
                    var rBService = $("#<%=ratBService.ClientID %> > a").attr("title");
                    var rBFamily = $("#<%=ratBFamily.ClientID %> > a").attr("title");
                    var rBCommunity = $("#<%=ratBCommunity.ClientID %> > a").attr("title");
                    //Current
                    var rCFriends = $("#<%=ratCFriends.ClientID %> > a").attr("title");
                    var rCSchwork = $("#<%=ratCSchwork.ClientID %> > a").attr("title");
                    var rCService = $("#<%=ratCService.ClientID %> > a").attr("title");
                    var rCFamily = $("#<%=ratCFamily.ClientID %> > a").attr("title");
                    var rCCommunity = $("#<%=ratCCommunity.ClientID %> > a").attr("title");

                    //BaseLine
                    if (rBFriends != 0) { $("#<%=ratBFriends.ClientID %> > a").attr("title", "Rated: " + rating[rBFriends]); }
                    else { $("#<%=ratBFriends.ClientID %> > a").attr("title", rating[rBFriends]); }

                    if (rBSchwork != 0) { $("#<%=ratBSchwork.ClientID %> > a").attr("title", "Rated: " + rating[rBSchwork]); }
                    else { $("#<%=ratBSchwork.ClientID %> > a").attr("title", rating[rBSchwork]); }

                    if (rBService != 0) { $("#<%=ratBService.ClientID %> > a").attr("title", "Rated: " + rating[rBService]); }
                    else { $("#<%=ratBService.ClientID %> > a").attr("title", rating[rBService]); }

                    if (rBFamily != 0) { $("#<%=ratBFamily.ClientID %> > a").attr("title", "Rated: " + rating[rBFamily]); }
                    else { $("#<%=ratBFamily.ClientID %> > a").attr("title", rating[rBFamily]); }

                    if (rBCommunity != 0) { $("#<%=ratBCommunity.ClientID %> > a").attr("title", "Rated: " + rating[rBCommunity]); }
                    else { $("#<%=ratBCommunity.ClientID %> > a").attr("title", rating[rBCommunity]); }

                    //Current
                    if (rCFriends != 0) { $("#<%=ratCFriends.ClientID %> > a").attr("title", "Rated: " + rating[rCFriends]); }
                    else { $("#<%=ratCFriends.ClientID %> > a").attr("title", rating[rCFriends]); }

                    if (rCSchwork != 0) { $("#<%=ratCSchwork.ClientID %> > a").attr("title", "Rated: " + rating[rCSchwork]); }
                    else { $("#<%=ratCSchwork.ClientID %> > a").attr("title", rating[rCSchwork]); }

                    if (rCService != 0) { $("#<%=ratCService.ClientID %> > a").attr("title", "Rated: " + rating[rCService]); }
                    else { $("#<%=ratCService.ClientID %> > a").attr("title", rating[rCService]); }

                    if (rCFamily != 0) { $("#<%=ratCFamily.ClientID %> > a").attr("title", "Rated: " + rating[rCFamily]); }
                    else { $("#<%=ratCFamily.ClientID %> > a").attr("title", rating[rCFamily]); }

                    if (rCCommunity != 0) { $("#<%=ratCCommunity.ClientID %> > a").attr("title", "Rated: " + rating[rCCommunity]); }
                    else { $("#<%=ratCCommunity.ClientID %> > a").attr("title", rating[rCCommunity]); }


                    $(function () {
                        $("#chkEnrollinsch").click(function () {
                            if (this.checked) {
                                $("#lblReqSchName").css('visibility', 'visible');
                                $("#lblCurrGradeReq").css('visibility', 'visible');
                                $("#lblSchYearReq").css('visibility', 'visible');
                                $("#lblEnrollInSch").css('visibility', 'hidden');
                                $(".enrollInSchGroup").removeAttr("disabled");
                                $("#chkGE").attr("disabled", false);
                                $("#chkSE").attr("disabled", false);
                                $("#chkIEPYes").attr("disabled", false);
                                $("#chkIEPNo").attr("disabled", false);
                                if ($("#chkGE").attr("checked")) {
                                    $("#ddlSchPlaType")[0].selectedIndex = 0;
                                    $("#ddlSchPlaType").attr("disabled", true);
                                }
                                else {
                                    $("#ddlSchPlaType").attr("disabled", false);
                                    $("#ddlSchPlaType")[0].selectedIndex = 0;
                                }
                            } else {
                                $("#lblReqSchName").css('visibility', 'hidden');
                                $("#lblCurrGradeReq").css('visibility', 'hidden');
                                $("#lblSchPlaTypeReq").css('visibility', 'hidden');
                                $("#lblSchYearReq").css('visibility', 'hidden');
                                $("#lblEnrollInSch").css('visibility', 'visible');
                                $(".enrollInSchGroup").attr("disabled", true);
                                $("#chkGE").attr("disabled", true);
                                $("#chkSE").attr("disabled", true);
                                $("#chkIEPYes").attr("disabled", true);
                                $("#chkIEPNo").attr("disabled", true);
                                $("#ddlSchPlaType")[0].selectedIndex = 0;
                                $("#ddlSchPlaType").attr("disabled", true);
                            }
                        });
                    });

                    $(function () {
                        $("#chkSE").click(function () {
                            if (this.checked) {
                                $("#lblSchPlaTypeReq").css('visibility', 'visible');
                                $("#ddlSchPlaType").attr("disabled", false);
                                $("#ddlSchPlaType")[0].selectedIndex = 0;
                                $("#chkGE").attr("checked", false);
                            } else {
                                $("#lblSchPlaTypeReq").css('visibility', 'hidden');
                                $("#ddlSchPlaType")[0].selectedIndex = 0;
                                $("#ddlSchPlaType").attr("disabled", true);
                                $("#chkGE").attr("checked", true);
                            }
                        });

                        $("#chkGE").click(function () {
                            if (this.checked) {
                                $("#lblSchPlaTypeReq").css('visibility', 'hidden');
                                $("#ddlSchPlaType")[0].selectedIndex = 0;
                                $("#ddlSchPlaType").attr("disabled", true);
                                $("#chkSE").attr("checked", false);
                            } else {
                                $("#lblSchPlaTypeReq").css('visibility', 'visible');
                                $("#ddlSchPlaType").attr("disabled", false);
                                $("#ddlSchPlaType")[0].selectedIndex = 0;
                                $("#chkSE").attr("checked", true);
                            }
                        });

                        $('.lnkUpdate').click(function () {
                            if ($(this).closest('tr:has(td)').find('select[id*=ddlOrg]').val() < 1 &&
                                $(this).closest('tr:has(td)').find('.lblCurrentStatus').html() == 'Enrolled') {
                                fnNotify('Please select an organization!');
                                return false;
                            }
                            else { return true; }
                        });
                    });

                    YouthWorkFlow(youthId);

                    $('.help_me').HelpDialog();

                    $('#ddlMemRel').change(function () {
                        var ddlMemRel = $('#ddlMemRel').val();
                        if (ddlMemRel == 'Birth Mother') {
                            $('#ddlFamGender').val('Female');
                        }
                        else if (ddlMemRel == 'Birth Father') {
                            $('#ddlFamGender').val('Male');
                        }
                        //                        else {
                        //                            $('#ddlFamGender').val('select');
                        //                        }
                    });

                    $('#ddlRelEd').change(function () {
                        var ddlRelEd = $('#ddlRelEd').val();
                        if (ddlRelEd == 'Birth Mother') {
                            $('#ddlFamGenderEdit').val('Female');
                        }
                        else if (ddlRelEd == 'Birth Father') {
                            $('#ddlFamGenderEdit').val('Male');
                        }
                        //                        else {
                        //                            $('#ddlFamGenderEdit').val('select');
                        //                        }
                    });

                    $('#ddlRelationship1').change(function () {
                        var ddlRelationship1 = $('#ddlRelationship1').val();
                        if (ddlRelationship1 == 'Birth Mother') {
                            $('#ddlRefP1Gender').val('Female');
                        }
                        else if (ddlRelationship1 == 'Birth Father') {
                            $('#ddlRefP1Gender').val('Male');
                        }
                        //                        else {
                        //                            $('#ddlRefP1Gender').val('select');
                        //                        }
                    });

                    $('#ddlRelationship2').change(function () {
                        var ddlRelationship2 = $('#ddlRelationship2').val();
                        if (ddlRelationship2 == 'Birth Mother') {
                            $('#ddlRefP2Gender').val('Female');
                        }
                        else if (ddlRelationship2 == 'Birth Father') {
                            $('#ddlRefP2Gender').val('Male');
                        }
                        //                        else {
                        //                            $('#ddlRefP2Gender').val('select');
                        //                        }
                    });

                    //if($('[id$=tblDashYouth]'))
                    if ($('[id$=tblDashYouth]').hasClass('KeyDashboard_Large')) { $('#tdYCDivider').css({ 'width': '150px' }); }
                    if ($('[id$=tblDashFamily]').hasClass('KeyDashboard_Large')) { $('#tdFCDivider').css({ 'width': '150px' }); }

                    $(document).on('click', function (e) {
                        if (!$.contains($('.Racepoup').get(0), e.target)) {
                            $('#divtxtEthinicity11').attr('Style', 'display:none');
                        }
                    });
                    $(".showrace").click(function (e) {
                        e.stopPropagation(); // This is the preferred method. 
                        return false;        // This should not be used unless you do not want 
                        // any click events registering inside the div 
                    });
                }

                function showPOCPrintPopup(lbPOCID) {
                    var hdnPOCId = document.getElementById('<%=hdnPOCId.ClientID %>');
                    var ChkBoth = document.getElementById('<%=chkFullPOC.ClientID%>');
                    var ChkBriefPOC = document.getElementById('<%=chkBriefPOC.ClientID%>');
                    ChkBoth.checked = true;
                    ChkBriefPOC.checked = false;
                    hdnPOCId.value = lbPOCID.getAttribute('POCID');
                    $find('mpePrint').show();
                    return false;
                }

                function CheckDateSigned() {
                    var selectedval = $("#ddlIsFidelitySigned").val();
                    var txtDateSigned = document.getElementById('txtDateSigned');
                    var lbltxtDateSignedreq = document.getElementById('lbltxtDateSignedreq');
                    if (selectedval == "0" || selectedval == "") {
                        txtDateSigned.disabled = true;
                        lbltxtDateSignedreq.style.display = 'none';
                        txtDateSigned.value = "__/__/____";
                    }
                    else {
                        txtDateSigned.disabled = false;
                        lbltxtDateSignedreq.style.display = '';
                    }
                }

                function PrintSelectCheck() {
                    var chkFullPOC = document.getElementById('<%=chkFullPOC.ClientID%>');
                    var hfPOCID = document.getElementById('<%=hdnPOCId.ClientID %>');
                    var ChkBriefPOC = document.getElementById('<%=chkBriefPOC.ClientID%>');
                    var hfspdoccid = document.getElementById('<%=hfspdocId.ClientID%>');
                    var pageURL = '', width = 800, height = 600;
                    var title = '';
                    if (chkFullPOC.checked) {
                        pageURL = '../POC/ViewPOCReport.aspx?P=' + hfPOCID.value + '&M=POCAIS&display=show&Did=' + hfspdoccid.value;
                        title = "Plan Of Care Report";
                    }
                    else if (ChkBriefPOC.checked) {
                        pageURL = '../POC/ViewPOCReport.aspx?P=' + hfPOCID.value + '&M=BriefPOC&display=show&Did=' + hfspdoccid.value;
                        title = "Plan Of Care Report";
                    }
                    else {
                        fnNotify("Please select any one option to Print!");
                    }
                    if (pageURL != '') {
                        setTargetFrame(pageURL, width, height, title);
                    }
                    return false;
                }
