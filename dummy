                                if (cblTeamItem.type == 'checkbox') {
                                    if (cblTeamItem.checked) {
                                        InviteesOK = true;
                                    }
                                }
                            }
                        }
                    }
                }
            }
            return InviteesOK;
        }

        function ChangedocHeaderChkbox() {
            // Whenever a checkbox in the GridView is toggled, we need to
            // check the Header checkbox if ALL of the GridView checkboxes are
            // checked, and uncheck it otherwise
            var count = 0;
            if (CheckBoxIDs != null) {
                // check to see if all other checkboxes are checked
                for (var i = 1; i < CheckBoxIDs.length; i++) {
                    var cb = document.getElementById(CheckBoxIDs[i]);
                    if (cb != null) {
                        if (!cb.checked) {
                            count++;
                        }
                    }
                }
                if (count > 0) {
                    // Whoops, there is an unchecked checkbox, make sure
                    // that the header checkbox is unchecked
                    ChangedocCheckBoxState(CheckBoxIDs[0], false);
                    return;
                }
                else {
                    // If we reach here, ALL GridView checkboxes are checked
                    ChangedocCheckBoxState(CheckBoxIDs[0], true);
                }
            }
        }

        function ChangedocCheckBoxState(id, checkState) {
            var cb = document.getElementById(id);
            if (cb != null)
                cb.checked = checkState;
        }

        function ChangeAlldocCheckBoxStates(checkState) {
            // Toggles through all of the checkboxes defined in the CheckBoxIDs array
            // and updates their value to the checkState input parameter
            if (CheckBoxIDs != null) {
                for (var i = 0; i < CheckBoxIDs.length; i++)
                    ChangedocCheckBoxState(CheckBoxIDs[i], checkState);
            }
        }

        function ValComposeMail() {
            var txtSubject = document.getElementById('<%=txtSubject.ClientID%>').value;
            var Contentmsg = $find('<%=hteMsgBody.ClientID%>').get_content();
            var lblErrorMessageText = document.getElementById('<%=lblErrorMessageText.ClientID%>');
            if (Page_ClientValidate("SendMail")) {
                if (txtSubject == '' && Contentmsg == '') {
                    lblErrorMessageText.innerHTML = "Do you want to send this message without subject and content?";
                    $find('mpMessages').show();
                    return false;
                }
                else if (txtSubject == '' && Contentmsg != '') {
                    lblErrorMessageText.innerHTML = "Do you want to send this message without a subject?";
                    $find('mpMessages').show();
                    return false;
                }
                else if (txtSubject != '' && Contentmsg == '') {
                    lblErrorMessageText.innerHTML = "Do you want to send this message without a content?";
                    $find('mpMessages').show();
                    return false;
                } else {
                    return true;
                }

            }
            else {
                return false;
            }
        }

        function ShowFamilyAdd() {
            $find("mpeFamilyAdd").show();
            $find("mpePCG").hide();
        }
        function ShowFamilyAddLG() {
            $find("mpeFamilyAdd").show();
            $find("mpeLG").hide();
        }
        function ShowFamilyAddPCGLG() {
            $find("mpeFamilyAdd").show();
            $find("mpePCGLG").hide();
            return false;
        }
        function ShowYouthEnrollmentHistory() {
            $find("mpeStatusHistory").show();
            $find("AddEnrollmentStatus").hide();
            return false;
        }
        function ShowAddStatus() {
            $find("mpeStatusHistory").hide();
            $find("AddEnrollmentStatus").show();
            var txtEnrolledDate = document.getElementById('<%= txtEnrolledDate.ClientID %>');
            var ddlEnrollmentStatus = document.getElementById('<%= ddlEnrollmentStatus.ClientID %>');
            var ddlStatusReason = document.getElementById('<%= ddlStatusReason.ClientID %>');
            var ddlYouthOrganization = document.getElementById('<%= ddlYouthOrganization.ClientID %>');
            txtEnrolledDate.value == "__/__/____";
            ddlEnrollmentStatus.selectedIndex = 0;
            ddlYouthOrganization.selectedIndex = 0;
            ddlStatusReason.selectedIndex = 0;
            return false;
        }

        function OnClientAsyncFileUploadComplete(sender, args) {
            var upload = document.getElementById('<%= lbUpload.ClientID %>');
            var cancel_upload = document.getElementById('<%=lbCancelUpload.ClientID %>');
            var fileContentLength = parseInt(args.get_length())
            if (fileContentLength < 5000005) {
                if (args.get_errorMessage() == null) {
                    $('#ctl00_MainContent_tcEnrollment_tpDocuments_afuDocument_ctl02').css("background-color", "#CCCCCC");
                    if (upload != null) {
                        upload.style.display = '';
                    }
                    if (cancel_upload != null) {
                        cancel_upload.style.display = '';
                    }
                }
                else { fnNotify(args.get_errorMessage()); }
            }
            else {
                $('#ctl00_MainContent_tcEnrollment_tpDocuments_afuDocument_ctl02').css("background-color", "Red");
                if (upload != null) {
                    upload.style.display = 'none';
                }
                if (cancel_upload != null) {
                    cancel_upload.style.display = 'none';
                }
                fnNotify('Maximum file size allowed is 5MB! Please choose another file to upload!');
            }
            if ($("#lbComposeMail") != null && $("#lbComposeMail") != undefined && !$("#lbComposeMail").hasClass("aspNetDisabled")) {
                $("#lbComposeMail").removeClass("disabled");
                $("#lbComposeMail").addClass("blue");
            }
            if ($("#lbComposeMaiNpPermission") != null && $("#lbComposeMaiNpPermission") != undefined && !$("#lbComposeMaiNpPermission").hasClass("aspNetDisabled")) {
                $("#lbComposeMaiNpPermission").removeClass("disabled");
                $("#lbComposeMaiNpPermission").addClass("blue");
            }
        }

        function OnClientUploadError(sender, args) {
            if (args.get_errorMessage() != null) {
                fnNotify('Maximum file size allowed is 5MB! Please choose another file to upload!');
            }
        }

        function OnClientUploadStarted(sender, args) {
            if ($("#lbComposeMail") != null && $("#lbComposeMail") != undefined && !$("#lbComposeMail").hasClass("aspNetDisabled")) {
                $("#lbComposeMail").removeClass("blue");
                $("#lbComposeMail").addClass("disabled");
            }
            if ($("#lbComposeMaiNpPermission") != null && $("#lbComposeMaiNpPermission") != undefined && !$("#lbComposeMaiNpPermission").hasClass("aspNetDisabled")) {
                $("#lbComposeMaiNpPermission").removeClass("disabled");
                $("#lbComposeMaiNpPermission").addClass("blue");
            }
        }

        function CheckButtonEnable() {
            if (!$("#lbComposeMail").hasClass("disabled")) {
                return true;
            }
            else {
                return false;
            }
        }

        function checkFamilyMember() {
            var ie = false;
            var Name = '';
            var Role = '';
            if (document.all) { ie = true; }
            var duplicateCount = 0;
            var family_ok = false;
            var familymember_grid = document.getElementById('<%=gvFamilyMembers.ClientID %>');
            var familyFirst_name = document.getElementById('<%=txtMemName.ClientID %>');
            var familyLast_name = document.getElementById('<%=txtMemLName.ClientID %>');
            var family_race = document.getElementById('<%=ddlFamilyRace.ClientID %>');
            var family_role = document.getElementById('<%=ddlMemRel.ClientID %>');
            var msg = document.getElementById('<%=lblFamilyMsg.ClientID %>');
            var family_Phone1 = document.getElementById('<%=txtContactPhone.ClientID %>').value;
            var famliy_PhoneType1 = document.getElementById('<%=ddlPhoneType1.ClientID %>').selectedIndex;
            var familymember_name = familyLast_name.value + ', ' + familyFirst_name.value;
            if (familyFirst_name.value.trim() == '') {
                family_ok = false;
                msg.innerHTML = 'First Name is required!';
                setTimeout("clearmsgtext()", 3000);
            }
            else if (familyLast_name.value.trim() == '') {
                family_ok = false;
                msg.innerHTML = 'Last Name is required!';
                setTimeout("clearmsgtext()", 3000);
            }
            else if (family_race.selectedIndex == 0) {
                family_ok = false;
                msg.innerHTML = 'Race is required!';
                setTimeout("clearmsgtext()", 3000);
            }
            else if (family_role.selectedIndex == 0) {
                family_ok = false;
                msg.innerHTML = 'Relationship is required!';
                setTimeout("clearmsgtext()", 3000);
            }
            else if (family_Phone1 != '(___) ___-____' && famliy_PhoneType1 == 0) {
                family_ok = false;
                msg.innerHTML = 'Phone 1 Type is required!';
                setTimeout("clearmsgtext()", 3000);
            }
            else {
                if (Page_ClientValidate("FamMemSave")) {
                    if (familymember_grid.rows.length > 0) {
                        for (var i = 1; i < familymember_grid.rows.length; i++) {
                            if (!ie) {
                                Name = familymember_grid.rows[i].cells[0].textContent;
                                Role = familymember_grid.rows[i].cells[1].textContent;
                            }
                            else {
                                Name = familymember_grid.rows[i].cells[0].innerText;
                                Role = familymember_grid.rows[i].cells[1].innerText;
                            }
                            if (Name == familymember_name) {
                                var familyrole = family_role.options[family_role.selectedIndex].text;
                                if (Role == familyrole) {
                                    duplicateCount++;
                                }
                            }
                        }
                        if (duplicateCount > 0) {
                            family_ok = false;
                            msg.innerHTML = 'A Family Member already exists with this name and role!';
                            setTimeout("clearmsgtext()", 3000);
                        }
                        else {
                            family_ok = true;
                            msg.innerHTML = '';
                        }
                    }
                    else {
                        family_ok = true;
                        msg.innerHTML = '';
                    }
                }
            }

            if (family_ok) {
                family_ok = checkActivePOC("FamliyMemberAdd");
                if (!family_ok) {
                    $find('mpeFamilyAdd').hide();
                }
            }
            return family_ok;
        }

        function clearmsgtext() {
            var msg = document.getElementById('<%=lblFamilyMsg.ClientID %>');
            msg.innerHTML = '';
        }



        function TempMsg(msg, msg_element) {
            msg_element.innerHTML = msg;
            var func = function () { ClearWorkFlowMessages(msg_element); };
            setTimeout(func, 2000);
            msg_element.className = 'WorkFlowMessage';
        }
        function ClearWorkFlowMessages(msg_element) {
            msg_element.innerHTML = '';
            msg_element.className = '';
        }
        function highlight(el) {
            var fn = function () { toggler(el); };
            myfn = setInterval(fn, 500);
        }
        function toggler(el) {
            if (el.className == '') { el.className = 'Highlight'; } else { el.className = ''; }
        }
        function TimeLine() {
            var youthId = '<%= Request.QueryString["YID"] %>';
            var youthName = document.getElementById('<%=lblNameDashboard.ClientID %>').innerHTML;
            var caseNumber = document.getElementById('<%=lblCaseNumberDashboard.ClientID %>').innerHTML;
            if (caseNumber != '') {
                window.location.href = 'Timeline.aspx?YID=' + youthId + '&YN=' + youthName + '&YC=' + caseNumber;
            }
            else {
                window.location.href = 'Timeline.aspx?YID=' + youthId + '&YN=' + youthName;
            }
        }

        function CopyYouthAddress(IsYouthAddress, Address) {
            //Youth Address
            var txtStreet1 = document.getElementById('<%=txtStreet111.ClientID %>');
            var txtStreet2 = document.getElementById('<%=txtStreet211.ClientID %>');
            var txtCity = document.getElementById('<%=txtCity11.ClientID %>');
            var ddlState = document.getElementById('<%=ddlState11.ClientID %>');
            var txtReferralZipCode = document.getElementById('<%=txtZip1.ClientID %>');

            switch (Address) {
                case "Address1": //Parent/Gaurdian Address 1
                    var txtStreet11 = document.getElementById('<%=txtStreet11.ClientID %>');
                    var txtStreet21 = document.getElementById('<%=txtStreet21.ClientID %>');
                    var txtcityRef1 = document.getElementById('<%=txtcityRef1.ClientID %>');
                    var ddlState111 = document.getElementById('<%=ddlState111.ClientID %>');
                    var txtZip11 = document.getElementById('<%=txtZip11.ClientID %>');


                    if (IsYouthAddress.checked) {
                        txtStreet11.value = txtStreet1.value;
                        txtStreet21.value = txtStreet2.value;
                        txtcityRef1.value = txtCity.value;
                        ddlState111.selectedIndex = ddlState.selectedIndex;
                        txtZip11.value = txtReferralZipCode.value;
                    }
                    else {
                        txtStreet11.value = '';
                        txtStreet21.value = '';
                        txtcityRef1.value = '';
                        ddlState111.selectedIndex = 0;
                        txtZip11.value = '_____-____';
                    }
                    break;
                case "Address2": //Parent/Gaurdian Address 2
                    var txtStreet12 = document.getElementById('<%=txtStreet12.ClientID %>');
                    var txtStreet22 = document.getElementById('<%=txtStreet22.ClientID %>');
                    var txtcityRef2 = document.getElementById('<%=txtcityRef2.ClientID %>');
                    var ddlState21 = document.getElementById('<%=ddlState21.ClientID %>');
                    var txtZip21 = document.getElementById('<%=txtZip21.ClientID %>');

                    if (IsYouthAddress.checked) {
                        txtStreet12.value = txtStreet1.value;
                        txtStreet22.value = txtStreet2.value;
                        txtcityRef2.value = txtCity.value;
                        ddlState21.selectedIndex = ddlState.selectedIndex;
                        txtZip21.value = txtReferralZipCode.value;
                    }
                    else {
                        txtStreet12.value = '';
                        txtStreet22.value = '';
                        txtcityRef2.value = '';
                        ddlState21.selectedIndex = 0;
                        txtZip21.value = '_____-____';
                    }
                    break;
                case "AddFamily": //Add Family Member Address
                    var txtMemStreet1 = document.getElementById('<%=txtMemStreet1.ClientID %>');
                    var txtMemStreet2 = document.getElementById('<%=txtMemStreet2.ClientID %>');
                    var txtMemCity = document.getElementById('<%=txtMemCity.ClientID %>');
                    var ddlMemState = document.getElementById('<%=ddlMemState.ClientID %>');
                    var txtMemZip1 = document.getElementById('<%=txtMemZip1.ClientID %>');
                    var inputs = IsYouthAddress.getElementsByTagName('input');

                    if (inputs != null) {
                        if (inputs[0].checked) { //Yes
                            txtMemStreet1.value = txtStreet1.value;
                            txtMemStreet2.value = txtStreet2.value;
                            txtMemCity.value = txtCity.value;
                            ddlMemState.selectedIndex = ddlState.selectedIndex;
                            txtMemZip1.value = txtReferralZipCode.value;
                        }
                        else {
                            txtMemStreet1.value = '';
                            txtMemStreet2.value = '';
                            txtMemCity.value = '';
                            ddlMemState.selectedIndex = 0;
                            txtMemZip1.value = '_____-____';
                        }
                    }
                    break;
                case "EditFamily": //Edit Family Member Address
                    var txtStreet1Ed = document.getElementById('<%=txtStreet1Ed.ClientID %>');
                    var txtStreet2Ed = document.getElementById('<%=txtStreet2Ed.ClientID %>');
                    var txtcityEd = document.getElementById('<%=txtcityEd.ClientID %>');
                    var ddlStateEd = document.getElementById('<%=ddlStateEd.ClientID %>');
                    var txtZip1Ed = document.getElementById('<%=txtZip1Ed.ClientID %>');
                    var inputs = IsYouthAddress.getElementsByTagName('input');

                    if (inputs != null) {
                        if (inputs[0].checked) { //Yes
                            txtStreet1Ed.value = txtStreet1.value;
                            txtStreet2Ed.value = txtStreet2.value;
                            txtcityEd.value = txtCity.value;
                            ddlStateEd.selectedIndex = ddlState.selectedIndex;
                            txtZip1Ed.value = txtReferralZipCode.value;
                        }
                        else {
                            txtStreet1Ed.value = '';
                            txtStreet2Ed.value = '';
                            txtcityEd.value = '';
                            ddlStateEd.selectedIndex = 0;
                            txtZip1Ed.value = '_____-____';
                        }
                    }
                    break;
            }
        }

        function ReferralOk() {
            var rOK = true;
            if (document.getElementById('<%=txtss.ClientID %>').value == '') { rOK = false; }
            if (rOK) { if (document.getElementById('<%=txtPrivateInsuranceName.ClientID %>').value == '') { rOK = false; } }
            if (rOK) {
                if (document.getElementById('<%=cbNo.ClientID %>').checked) { rOK = true; }
                else {
                    if (document.getElementById('<%=cbYes.ClientID %>').checked) {
                        if (document.getElementById('<%=txtAgency.ClientID %>').value == '') { rOK = false; }
                        else { rOK = true; }
                    }
                }
            }

            if (rOK) { if (document.getElementById('<%=txtDJSDSSName.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if ((document.getElementById('<%=txtDJSDSSPhone.ClientID %>').value == '') || (document.getElementById('<%=txtDJSDSSPhone.ClientID %>').value == '(___)___-____')) { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=txtDJSDSSEmail.ClientID %>').value == '') { rOK = false; } }

            if (rOK) { if (document.getElementById('<%=txtLastName1.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=txtFirstName1.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=ddlRelationship1.ClientID %>').selectedIndex == 0) { rOK = false; } }
            if (rOK) { if ((document.getElementById('<%=txtWorkPhone.ClientID %>').value == '') || (document.getElementById('<%=txtWorkPhone.ClientID %>').value == '(___)___-____')) { rOK = false; } }
            if (rOK) { if ((document.getElementById('<%=txtContactPhone1.ClientID %>').value == '') || (document.getElementById('<%=txtContactPhone1.ClientID %>').value == '(___)___-____')) { rOK = false; } }

            if (rOK) { if (document.getElementById('<%=txtLastName2.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=txtFirstName2.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=ddlRelationship2.ClientID %>').selectedIndex == 0) { rOK = false; } }
            if (rOK) { if ((document.getElementById('<%=txtWorkPhone2.ClientID %>').value == '') || (document.getElementById('<%=txtWorkPhone2.ClientID %>').value == '(___)___-____')) { rOK = false; } }
            if (rOK) { if ((document.getElementById('<%=txtContactPhone2.ClientID %>').value == '') || (document.getElementById('<%=txtContactPhone2.ClientID %>').value == '(___)___-____')) { rOK = false; } }

            if (rOK) { if (document.getElementById('<%=txtIssuesExplanation.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=txtReferringAgency.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=txtBriefHistory.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=txtDesiredOutcome.ClientID %>').value == '') { rOK = false; } }

            if (rOK) { if (document.getElementById('<%=hdfCurServiceId1.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=hdfCurServiceId2.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=hdfCurServiceId3.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=hdfCurServiceId4.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=hdfCurServiceId5.ClientID %>').value == '') { rOK = false; } }

            if (rOK) { if (document.getElementById('<%=hdfPreServiceId1.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=hdfPreServiceId2.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=hdfPreServiceId3.ClientID %>').value == '') { rOK = false; } }

            if (rOK) { if (document.getElementById('<%=txtAssessmentConduct.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=txtDSMIVDiagnosis.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=txtSchoolContactInfo.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if ((document.getElementById('<%=txtDateDiagnosis.ClientID %>').value == '') || (document.getElementById('<%=txtDateDiagnosis.ClientID %>').value == '__/__/____')) { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=txtGAFCurrent.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=txtGAFHighest.ClientID %>').value == '') { rOK = false; } }

            if (rOK) { if (document.getElementById('<%=txtAxisI.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=txtAxisII.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=txtAxisIII.ClientID %>').value == '') { rOK = false; } }
            if (rOK) { if (document.getElementById('<%=txtAxisIV.ClientID %>').value == '') { rOK = false; } }
            return rOK;
        }

        function ValidateSchoolDetails() {
            var school_ok = false;
            var msg = '';
            var start_date = document.getElementById('<%=txtSchStartDate.ClientID %>');
            var IsEnroll_School = document.getElementById('<%=chkEnrollinsch.ClientID %>');
            var school_name = document.getElementById('<%=txtSchName.ClientID %>');
            var current_grade = document.getElementById('<%=ddlCurrGrade.ClientID %>');
            var general_edu = document.getElementById('<%=chkGE.ClientID %>');
            var special_edu = document.getElementById('<%=chkSE.ClientID %>');
            var school_placement = document.getElementById('<%=ddlSchPlaType.ClientID %>');
            var school_year = document.getElementById('<%=ddlSchYear.ClientID %>');

            if (start_date.value != '__/__/____' && start_date.value != '') {
                if (CheckSchoolStartDate()) {
                    if (IsEnroll_School.checked) {
                        if (school_name.value != '') {
                            if (current_grade.selectedIndex != 0) {
                                if (special_edu.checked) {
                                    if (school_placement.selectedIndex != 0) {
                                        school_ok = true;
                                    }
                                    else {
                                        school_ok = false;
                                        msg = 'School Placement Type is required!';
                                    }
                                }
                                else {
                                    school_ok = true;
                                }
                                if (school_ok) {
                                    school_ok = false;
                                    if (school_year.selectedIndex != 0) {
                                        school_ok = true;
                                    }
                                    else {
                                        school_ok = false;
                                        msg = 'School Year is required!';
                                    }
                                }
                            }
                            else {
                                school_ok = false;
                                msg = 'Current Grade is required!';
                            }
                        }
                        else {
                            school_ok = false;
                            msg = 'School Name is required!';
                        }
                    }
                    else {
                        school_ok = true;
                    }
                }
                else {
                    school_ok = false;
                    msg = 'Start Date cannot be a future date!';
                }
            }
            else {
                school_ok = false;
                msg = 'Start Date is required!';
            }
            if (!school_ok) {
                fnNotify(msg);
            }
            return school_ok;
        }

        function CheckStatusDetails() {
            var enrollment = document.getElementById('<%=ddlEnrollmentStatus.ClientID %>');
            var enrollment_status = document.getElementById('<%=ddlEnrollmentStatus.ClientID %>').selectedIndex;
            var enrollmentStatusControl = document.getElementById('<%=ddlEnrollmentStatus.ClientID %>');
            var enrollmentStatusValue = enrollmentStatusControl.options[enrollmentStatusControl.selectedIndex].value;
            var enrollment_date = document.getElementById('<%=txtEnrolledDate.ClientID %>').value;
            var status_reason = document.getElementById('<%=ddlStatusReason.ClientID %>');
            var organisation = document.getElementById('<%=ddlYouthOrganization.ClientID %>');
            var DischargeText = document.getElementById('<%=hfDischargeText.ClientID %>').value;
            var continue_check = true;

            if (continue_check == true && enrollment_date == '__/__/____') { fnNotify('Please set start date!'); continue_check = false; }
            if (continue_check == true && enrollment_status == 0) { fnNotify('Please select an enrollment status!'); continue_check = false; }
            if (continue_check == true && (enrollmentStatusValue != 'selected' && enrollmentStatusValue != DischargeText && enrollmentStatusValue != 'Not Enrolled' && enrollmentStatusValue != 'Pending') && organisation.selectedIndex == 0) { fnNotify('Please select an organization!'); continue_check = false; }
            if (continue_check == true && enrollment_status > 2) { if (status_reason.selectedIndex == 0) { fnNotify('Please select a reason!'); continue_check = false; } }
            return continue_check;
        }

        function ClearStatusDetails() {
            document.getElementById('<%=txtEnrolledDate.ClientID %>').value = '__/__/____';
            document.getElementById('<%=ddlEnrollmentStatus.ClientID %>').selectedIndex = 0;
            document.getElementById('<%=ddlStatusReason.ClientID %>').disabled = true;
            ShowYouthEnrollmentHistory();
            return false;
        }

