            document.getElementById("<%=txtMAStreet2.ClientID %>").value = "";
            document.getElementById("<%=txtMACity.ClientID %>").value = "";
            document.getElementById("<%=txtMAZip.ClientID %>").value = "_____-____";
            document.getElementById("<%=ddlMAState.ClientID %>").selectedIndex = 0;
            ClearAllTeamMembers();
        }

        function CheckInvitees() {
