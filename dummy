            return false;
        }
        function YFValidate() {
            var cont = true;
            var eDate = document.getElementById('<%=txtYFEndDate.ClientID %>').value.trim();
            if (Page_ClientValidate("YFunding")) {
                if (Page_ClientValidate("YFStartDate")) {
                    if (eDate != '' && eDate != '__/__/____') {
                        if (Page_ClientValidate("YFEndDate")) {
                            var sDate = document.getElementById('<%=txtYFStartDate.ClientID %>').value;
                            if (dCompare(sDate, eDate)) { cont = true; }
                            else { cont = false; fnNotify('End Date should be after Start Date!'); }
                        }
                        else {
                            cont = false;
                        }
                    }
                }
                else {
                    cont = false;
                }
            }
            else {
                cont = false;
            }
            return cont;
        }
        function CheckSchoolStartDate() {
            var current = new Date();
            var currentyear = current.getFullYear();
            var currentmonth = current.getMonth();
            currentmonth = currentmonth + 1;
            var currentday = current.getDate();
            var cmonth = '';
            var cday = '';
            if (String(currentmonth).length == 1) { cmonth = (new String('0') + new String(currentmonth)); }
            else { cmonth = new String(currentmonth); }
            if (String(currentday).length == 1) { cday = (new String('0') + new String(currentday)); }
            else { cday = new String(currentday); }
            var c_date = new String(new String(currentyear) + cmonth + cday);
            c_date = parseInt(c_date);
            var date = document.getElementById('<%=txtSchStartDate.ClientID %>').value;
            var split_dob = date.split("/");
            var e_date = new String(split_dob[2] + split_dob[0] + split_dob[1]);
            e_date = parseInt(e_date);
            if (e_date > c_date) {
                fnNotify('Start Date cannot be a future date!');
                document.getElementById('<%=txtSchStartDate.ClientID %>').focus();
                return false;
            }
            else {
                return true;
            }
        }

        function CheckAddedDate() {
            var current = new Date();
            var currentyear = current.getFullYear();
            var currentmonth = current.getMonth();
            currentmonth = currentmonth + 1;
            var currentday = current.getDate();
            var cmonth = '';
            var cday = '';
            if (String(currentmonth).length == 1) { cmonth = (new String('0') + new String(currentmonth)); }
            else { cmonth = new String(currentmonth); }
            if (String(currentday).length == 1) { cday = (new String('0') + new String(currentday)); }
            else { cday = new String(currentday); }
            var c_date = new String(new String(currentyear) + cmonth + cday);
            c_date = parseInt(c_date);
            var date = document.getElementById('<%=txtStartDateAdd.ClientID %>').value;
            var split_dob = date.split("/");
            var e_date = new String(split_dob[2] + split_dob[0] + split_dob[1]);
            e_date = parseInt(e_date);
            if (e_date > c_date) {
                fnNotify('Date Added cannot be a future date!');
                document.getElementById('<%=txtStartDateAdd.ClientID %>').focus();
                return false;
            }
            else {
                return true;
            }
        }

        function ValidateRequiredElements(args) {
            var newRecord_ok = false;
            var chkphone1 = true;
            var isConcentchecked = false;
            var chkphoneType1 = true;
            var chkphone2 = true;
            var chkphoneType2 = true;
            var chkzip1 = true;
            var chkdob = true;
            var filledDataCount = 0;
            var RequiredElements = document.getElementById('hdfRequiredElements').value;
            var RequiredElementCount = (RequiredElements.split("|")).length - 1;
            var fname = document.getElementById('txtFName11').value;
            var nmiddle = document.getElementById('txtMiddle11').value;
            var lname = document.getElementById('txtLName11').value;
            var dob = document.getElementById('txtDOB11').value;
            var Street1 = document.getElementById('txtStreet111').value;
            var Street2 = document.getElementById('txtStreet211').value;
            var City = document.getElementById('txtCity11').value;
            var gender = document.getElementById('ddlGender11').selectedIndex;
            var State = document.getElementById('ddlState11').selectedIndex;
            var zip_ok = document.getElementById('txtZip1').value;
            var Case_Number = document.getElementById('txtCaseNo11').value;
            var Email = document.getElementById('txtEmail11').value;
            var txtMedicaidNo11 = document.getElementById('<%= txtMedicaidNo11.ClientID %>');
            var Medicaid = txtMedicaidNo11.value;
            var Phone1 = document.getElementById('txtYouthPhoneNo').value;
            var Phone2 = document.getElementById('txtYouthContactPhone').value;
            var PhoneType1 = document.getElementById('ddlYouthPhoneType1').selectedIndex;
            var PhoneType2 = document.getElementById('ddlYouthPhoneType2').selectedIndex;
            var ddlIsFidelitySignedind = document.getElementById('ddlIsFidelitySigned').selectedIndex;
            var txtDateSigned = document.getElementById('txtDateSigned');
            var ddlEthinicityNew = document.getElementById('ddlEthnicitynew');
            var ddlSmokingStatus = document.getElementById('ddlSmokingStatus');
            var hasm17module = document.getElementById('hfhasm17module').value;
            var youthage = document.getElementById('hfyouthage').value;

            //if (RequiredElements.indexOf("Ethnicity") > 0) {
            //    RequiredElementCount -= 1;
            //}

            if ((RequiredElements.indexOf("Ethnicity")) != -1) {
                if (hasm17module == 'True' && ddlEthinicityNew != null && ddlEthinicityNew != undefined) {
                    if (ddlEthinicityNew.selectedIndex == 0) {
                        fnNotify('Ethnicity is required!');
                        document.getElementById('ddlEthnicitynew').focus();
                        newRecord_ok = false;
                    }
                    else {
                        newRecord_ok = true;
                        filledDataCount++;
                    }
                } else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }

            if ((RequiredElements.indexOf("Youth Smoking Status")) != -1) {
                if (hasm17module == 'True' && parseInt(youthage) > 13 && ddlSmokingStatus != null && ddlSmokingStatus != undefined) {
                    if (ddlSmokingStatus.selectedIndex == 0) {
                        fnNotify('Youth Smoking Status is required!');
                        document.getElementById('ddlSmokingStatus').focus();
                        newRecord_ok = false;
                    }
                    else {
                        newRecord_ok = true;
                        filledDataCount++;
                    }
                } else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }

            if (ddlIsFidelitySignedind == 0) {
                fnNotify('A response to “Consent for Fidelity Assessment signed?” is required!');
                document.getElementById('ddlIsFidelitySigned').focus();
                newRecord_ok = false;
            }
            else {
                if (ddlIsFidelitySignedind == 1) {
                    newRecord_ok = true;
                    isConcentchecked = true;
                }
                else {
                    if (txtDateSigned.value == '__/__/____') {
                        fnNotify('Date Consent for Fidelity Assessment signed is required!');
                        txtDateSigned.focus();
                        newRecord_ok = false;
                    }
                    else if (!CheckDateSignedDate(txtDateSigned)) {
                        txtDateSigned.focus();
                        newRecord_ok = false;
                    }
                    else {
                        newRecord_ok = true;
                        isConcentchecked = true;
                    }
                }
            }

            if ((RequiredElements.indexOf("Email")) != -1) {
                if (Email.trim() == "") {
                    fnNotify('Email is required!');
                    document.getElementById('txtEmail11').focus();
                    newRecord_ok = false;
                } else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }

            if ((RequiredElements.indexOf("Phone 2")) != -1) {
                if (Phone2 == "(___) ___-____") {
                    fnNotify('Phone Number 2 is required!');
                    document.getElementById('txtYouthContactPhone').focus();
                    newRecord_ok = false;
                }
                else {
                    if (Check_PhoneAndFaxNo(document.getElementById('txtYouthContactPhone'))) {
                        if (PhoneType2 == 0) {
                            fnNotify('Phone 2 Type is required!');
                            document.getElementById('ddlYouthPhoneType2').focus();
                            newRecord_ok = false;
                        } else {
                            newRecord_ok = true;
                            filledDataCount++;
                        }
                    }
                    else {
                        newRecord_ok = false;
                    }
                }
            }
            else {
                if (Phone2 != "(___) ___-____") {
                    chkphone2 = Check_PhoneAndFaxNo(document.getElementById('txtYouthContactPhone'));
                }
            }

            if ((RequiredElements.indexOf("Phone 1")) != -1) {
                if (Phone1 == "(___) ___-____") {
                    fnNotify('Phone Number 1 is required!');
                    document.getElementById('txtYouthPhoneNo').focus();
                    newRecord_ok = false;
                }
                else {
                    if (Check_PhoneAndFaxNo(document.getElementById('txtYouthPhoneNo'))) {
                        if (PhoneType1 == 0) {
                            fnNotify('Phone 1 Type is required!');
                            document.getElementById('ddlYouthPhoneType1').focus();
                            newRecord_ok = false;
                        } else {
                            newRecord_ok = true;
                            filledDataCount++;
                        }
                    }
                    else {
                        newRecord_ok = false;
                    }
                }
            }
            else {
                if (Phone1 != "(___) ___-____") {
                    chkphone1 = Check_PhoneAndFaxNo(document.getElementById('txtYouthPhoneNo'));
                    if (chkphone1) {
                        if (PhoneType1 == 0) {
                            fnNotify('Phone 1 Type is required!');
                            document.getElementById('ddlYouthPhoneType1').focus();
                            chkphoneType1 = false;
                        }
                    }
                }
            }
            if ((RequiredElements.indexOf("Medicaid Number")) != -1) {
                if (Medicaid == "") {
                    fnNotify('Medicaid Number is required!');
                    document.getElementById('txtMedicaidNo11').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("Case Number")) != -1) {
                if (Case_Number == "") {
                    fnNotify('Case Number is required!');
                    document.getElementById('txtCaseNo11').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("Zipcode")) != -1) {
                if (zip_ok == "_____-____") {
                    fnNotify('Zip Code is required!');
                    document.getElementById('txtZip1').focus();
                    newRecord_ok = false;
                }
                else {
                    if (check_zipcodeNo(document.getElementById('txtZip1'))) {
                        newRecord_ok = true;
                        filledDataCount++;
                    }
                    else {
                        newRecord_ok = false;
                    }
                }
            }
            else {
                if (zip_ok != "_____-____") {
                    chkzip1 = check_zipcodeNo(document.getElementById('txtZip1'));
                }
            }

            if ((RequiredElements.indexOf("State")) != -1) {
                if (State == 0) {
                    fnNotify('State is required!');
                    document.getElementById('ddlState11').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("City")) != -1) {
                if (City.trim() == "") {
                    fnNotify('City is required!');
                    document.getElementById('txtCity11').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("Street 2")) != -1) {
                if (Street2.trim() == "") {
                    fnNotify('Street 2 is required!');
                    document.getElementById('txtStreet211').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("Street 1")) != -1) {
                if (Street1.trim() == "") {
                    fnNotify('Street 1 is required!');
                    document.getElementById('txtStreet111').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("Date of Birth")) != -1) {
                if (dob == '__/__/____') {
                    fnNotify('Date of Birth is required!');
                    document.getElementById('txtDOB11').focus();
                    newRecord_ok = false;
                }
                else {
                    if (dob != '__/__/____') {
                        if (Checkdob(document.getElementById('txtDOB11'))) {
                            newRecord_ok = true;
                            filledDataCount++;
                        }
                        else {
                            newRecord_ok = false;
                        }
                    }

                }
            }
            else {
                if (dob != '__/__/____') {
                    chkdob = Checkdob(document.getElementById('txtDOB11'));
                }
                else {
                    if (dob == '__/__/____') {
                        document.getElementById('txtDOB11').value = "00/00/0000";
                    }
                    newRecord_ok = true;
                }
            }

            if ((RequiredElements.indexOf("Race")) != -1) {
                if (!CheckRace()) {
                    fnNotify('Race is required!');
                    document.getElementById('<%= txtEthinicity11.ClientID %>').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("Gender")) != -1) {
                if (gender == 0) {
                    fnNotify('Gender is required!');
                    document.getElementById('ddlGender11').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("Last Name")) != -1) {
                if (lname.trim() == "") {
                    fnNotify('Last Name is required!');
                    document.getElementById('txtLName11').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("Middle Initial")) != -1) {
                if (nmiddle.trim() == "") {
                    fnNotify('Middle Initial is required!');
                    document.getElementById('txtMiddle11').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("First Name")) != -1) {
                if (fname.trim() == "") {
                    fnNotify('First Name is required!');
                    document.getElementById('txtFName11').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }


            if (RequiredElementCount == filledDataCount && chkphone1 == true && chkphone2 == true && chkzip1 == true && chkdob == true && chkphoneType1 == true && chkphoneType2 == true && isConcentchecked == true) {
                if (Page_ClientValidate('ValidateEmail')) {
                    var hfYouthUname = document.getElementById('<%=hfUserName.ClientID %>').value;
                    var txtEmail11 = document.getElementById('<%=txtEmail11.ClientID %>').value;
                    if (hfYouthUname != "0" && txtEmail11 == "") {
                        fnNotify("Email Address shouldn't empty, Because client portal login already created for this Youth!");
                        document.getElementById('<%=txtEmail11.ClientID %>').focus();
                        newRecord_ok = false;
                    } else {
                    newRecord_ok = true;
                }
                }
                else {
                    newRecord_ok = false;
                }
            }
            else {
                newRecord_ok = false;
            }
            if (newRecord_ok) {
                newRecord_ok = CheckMedicaidNumber(txtMedicaidNo11);
            }
            if (newRecord_ok) {
                if (args == "Youth") {
                    newRecord_ok = checkActivePOC("Youth");
                } else {
                    newRecord_ok = true;
                }
            }
            return newRecord_ok;
        }

        function Checkdob(Dateofbirth) {
            var dob_ok = false;
            var dob = Dateofbirth.value;
            var current = new Date();
            var currentyear = current.getFullYear();
            var currentmonth = current.getMonth();
            currentmonth = currentmonth + 1;
            var currentday = current.getDate();
            var cmonth = '';
            var cday = '';
            if (String(currentmonth).length == 1) { cmonth = (new String('0') + new String(currentmonth)); }
            else { cmonth = new String(currentmonth); }
            if (String(currentday).length == 1) { cday = (new String('0') + new String(currentday)); }
            else { cday = new String(currentday); }
            var c_date = new String(new String(currentyear) + cmonth + cday);
            c_date = parseInt(c_date);
            var split_dob = dob.split("/");
            var e_date = new String(split_dob[2] + split_dob[0] + split_dob[1]);
            e_date = parseInt(e_date);
            if (e_date == c_date || e_date > c_date) {
                fnNotify('Date of birth cannot be a future date or current date!');
                Dateofbirth.focus();
                dob_ok = false;
            }
            else {
                if (checkValidDate(Dateofbirth, "Date of Birth entered")) {
                    dob_ok = true;
                }
                else {
                    dob_ok = false;
                }
            }
            return dob_ok;
        }


        function ValidateReferralRequiredElements() {
            var newRecord_ok = false;
            var chkphone1 = true;
            var chkphoneType1 = true;
            var chkphone2 = true;
            var chkphoneType2 = true;
            var chkzip1 = true;
            var chkdob = true;
            var filledDataCount = 0;
            var RequiredElements = document.getElementById('hdfRequiredElements').value;
            var RequiredElementCount = (RequiredElements.split("|")).length - 1;
            var fname = document.getElementById('txtFName').value;
            var nmiddle = document.getElementById('txtMiddle').value;
            var lname = document.getElementById('txtLName').value;
            var dob = document.getElementById('txtDOB').value;
            var Street1 = document.getElementById('txtStreet1').value;
            var Street2 = document.getElementById('txtStreet2').value;
            var City = document.getElementById('txtCity').value;
            var gender = document.getElementById('ddlGender').selectedIndex;
            var Race = document.getElementById('ddlEthinicity').selectedIndex;
            var State = document.getElementById('ddlState').selectedIndex;
            var zip_ok = document.getElementById('txtReferralZipCode').value;
            var Case_Number = document.getElementById('txtCaseNo').value;
            var Medicaid = document.getElementById('txtMedicaidNo').value;
            var Phone1 = document.getElementById('txtRefferralYouthPhoneNo').value;
            var Phone2 = document.getElementById('txtRefYouthContactPhone').value;
            var RefEmail = document.getElementById('txtRefEmail').value;
            var PhoneType1 = document.getElementById('ddlRefPhoneType1').selectedIndex;
            var PhoneType2 = document.getElementById('ddlRefPhoneType2').selectedIndex;

            if (RequiredElements.indexOf("Ethnicity") > 0) {
                RequiredElementCount -= 1;
            }

            if ((RequiredElements.indexOf("Email")) != -1) {
                if (RefEmail.trim() == "") {
                    fnNotify('Email is required!');
                    document.getElementById('txtRefEmail').focus();
                    newRecord_ok = false;
                } else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }

            if ((RequiredElements.indexOf("Phone 2")) != -1) {
                if (Phone2 == "(___) ___-____") {
                    fnNotify('Phone Number 2 is required!');
                    document.getElementById('txtRefYouthContactPhone').focus();
                    newRecord_ok = false;
                }
                else {
                    if (Check_PhoneAndFaxNo(document.getElementById('txtRefYouthContactPhone'))) {
                        if (PhoneType2 == 0) {
                            fnNotify('Phone 2 Type is required!');
                            document.getElementById('ddlRefPhoneType2').focus();
                            newRecord_ok = false;
                        } else {
                            newRecord_ok = true;
                            filledDataCount++;
                        }
                    }
                    else {
                        newRecord_ok = false;
                    }
                }
            }
            else {
                if (Phone2 != "(___) ___-____") {
                    chkphone2 = Check_PhoneAndFaxNo(document.getElementById('txtRefYouthContactPhone'));
                }
            }

            if ((RequiredElements.indexOf("Phone 1")) != -1) {
                if (Phone1 == "(___) ___-____") {
                    fnNotify('Phone Number 1 is required!');
                    document.getElementById('txtRefferralYouthPhoneNo').focus();
                    newRecord_ok = false;
                }
                else {
                    if (Check_PhoneAndFaxNo(document.getElementById('txtRefferralYouthPhoneNo'))) {
                        if (PhoneType1 == 0) {
                            fnNotify('Phone 1 Type is required!');
                            document.getElementById('ddlYouthPhoneType1').focus();
                            newRecord_ok = false;
                        } else {
                            newRecord_ok = true;
                            filledDataCount++;
                        }
                    }
                    else {
                        newRecord_ok = false;
                    }
                }
            }
            else {
                if (Phone1 != "(___) ___-____") {
                    chkphone1 = Check_PhoneAndFaxNo(document.getElementById('txtRefferralYouthPhoneNo'));
                    if (chkphone1) {
                        if (PhoneType1 == 0) {
                            fnNotify('Phone 1 Type is required!');
                            document.getElementById('ddlYouthPhoneType1').focus();
                            chkphoneType1 = false;
                        }
                    }
                }
            }
            if ((RequiredElements.indexOf("Medicaid Number")) != -1) {
                if (Medicaid == "") {
                    fnNotify('Medicaid Number is required!');
                    document.getElementById('txtMedicaidNo').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("Case Number")) != -1) {
                if (Case_Number == "") {
                    fnNotify('Case Number is required!');
                    document.getElementById('txtCaseNo').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("Zipcode")) != -1) {
                if (zip_ok == "_____-____") {
                    fnNotify('Zip Code is required!');
                    document.getElementById('txtReferralZipCode').focus();
                    newRecord_ok = false;
                }
                else {
                    if (check_zipcodeNo(document.getElementById('txtReferralZipCode'))) {
                        newRecord_ok = true;
                        filledDataCount++;
                    }
                    else {
                        newRecord_ok = false;
                    }
                }
            }
            else {
                if (zip_ok != "_____-____") {
                    chkzip1 = check_zipcodeNo(document.getElementById('txtReferralZipCode'));
                }
            }

            if ((RequiredElements.indexOf("State")) != -1) {
                if (State == 0) {
                    fnNotify('State is required!');
                    document.getElementById('ddlState').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("City")) != -1) {
                if (City.trim() == "") {
                    fnNotify('City is required!');
                    document.getElementById('txtCity').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("Street 2")) != -1) {
                if (Street2.trim() == "") {
                    fnNotify('Street 2 is required!');
                    document.getElementById('txtStreet2').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("Street 1")) != -1) {
                if (Street1.trim() == "") {
                    fnNotify('Street 1 is required!');
                    document.getElementById('txtStreet1').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("Date of Birth")) != -1) {
                if (dob == '__/__/____') {
                    fnNotify('Date of Birth is required!');
                    document.getElementById('txtDOB').focus();
                    newRecord_ok = false;
                }
                else {
                    if (dob != '__/__/____') {
                        if (Checkdob(document.getElementById('txtDOB'))) {
                            newRecord_ok = true;
                            filledDataCount++;
                        }
                        else {
                            newRecord_ok = false;
                        }
                    }

                }
            }
            else {
                if (dob != '__/__/____') {
                    chkdob = Checkdob(document.getElementById('txtDOB'));
                }
                else {
                    document.getElementById('txtDOB').value = "00/00/0000";
                    newRecord_ok = true;
                }
            }

            if ((RequiredElements.indexOf("Race")) != -1) {
                if (Race == 0) {
                    fnNotify('Race is required!');
                    document.getElementById('ddlEthinicity').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("Gender")) != -1) {
                if (gender == 0) {
                    fnNotify('Gender is required!');
                    document.getElementById('ddlGender').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("Last Name")) != -1) {
                if (lname.trim() == "") {
                    fnNotify('Last Name is required!');
                    document.getElementById('txtLName').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("Middle Initial")) != -1) {
                if (nmiddle.trim() == "") {
                    fnNotify('Middle Initial is required!');
                    document.getElementById('txtMiddle').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if ((RequiredElements.indexOf("First Name")) != -1) {
                if (fname.trim() == "") {
                    fnNotify('First Name is required!');
                    document.getElementById('txtFName').focus();
                    newRecord_ok = false;
                }
                else {
                    newRecord_ok = true;
                    filledDataCount++;
                }
            }
            if (RequiredElementCount == filledDataCount && chkphone1 == true && chkphone2 == true && chkzip1 == true && chkdob == true && chkphoneType1 == true && chkphoneType2 == true) {
                return newRecord_ok;
            }
            else {
                return false;
            }
        }

        function ValidtaionYouthRecord() {
            var fname1 = document.getElementById('<%=txtFirstName1.ClientID %>').value;
            var lname1 = document.getElementById('<%=txtLastName1.ClientID %>').value;
            var fname2 = document.getElementById('<%=txtFirstName2.ClientID %>').value;
            var lname2 = document.getElementById('<%=txtLastName2.ClientID %>').value;
            var ddlEthnicity1 = document.getElementById('<%=ddlEthnicity1.ClientID %>').selectedIndex;
            var ddlEthnicity2 = document.getElementById('<%=ddlEthnicity2.ClientID %>').selectedIndex;
            var Relationship1 = document.getElementById('<%=ddlRelationship1.ClientID %>').selectedIndex;
            var Relationship2 = document.getElementById('<%=ddlRelationship2.ClientID %>').selectedIndex;
            var member_role1 = document.getElementById('<%=ddlRelationship1.ClientID %>');
            var member_role2 = document.getElementById('<%=ddlRelationship2.ClientID %>');

            var txtMedicaidNo = document.getElementById('<%=txtMedicaidNo.ClientID %>');

            if (Page_ClientValidate("ReferralSave")) {
                if (ValidateReferralRequiredElements()) {
                    if (check_ssnnumber(document.getElementById('<%=txtss.ClientID %>')) == false) {
                        return false;
                    }

                    else if (!CheckMedicaidNumber(txtMedicaidNo)) {
                        return false;
                    }

                    else {
                        // If lastname or firstname filled in Parent1 in Referral Details. Then user must fill other details like Race & Relationship
                        if (lname1.trim() != "" || fname1.trim() != "") {
                            if (fname1.trim() == "") {
                                fnNotify('First Name is required in Parent1!');
                                document.getElementById('<%=txtFirstName1.ClientID %>').focus();
                                return false;
                            }
                            if (lname1.trim() == "") {
                                fnNotify('Last Name is required in Parent1!');
                                document.getElementById('<%=txtLastName1.ClientID %>').focus();
                                return false;
                            }
                            if (ddlEthnicity1 == 0) {
                                fnNotify('Race is required in Parent1!');
                                document.getElementById('<%=ddlEthnicity1.ClientID %>').focus();
                                return false;
                            }
                            if (Relationship1 == 0) {
                                fnNotify('Relationship is required in Parent1!');
                                document.getElementById('<%=ddlRelationship1.ClientID %>').focus();
                                return false;
                            }
                        }

                        // If lastname or firstname filled in Parent2 in Referral Details. Then user must fill other details like Race & Relationship
                        if (lname2.trim() != "" || fname2.trim() != "") {
                            if (fname2.trim() == "") {
                                fnNotify('First Name is required in Parent2!');
                                document.getElementById('<%=txtFirstName2.ClientID %>').focus();
                                return false;
                            }
                            if (lname2.trim() == "") {
                                fnNotify('Last Name is required in Parent2!');
                                document.getElementById('<%=txtLastName2.ClientID %>').focus();
                                return false;
                            }
                            if (ddlEthnicity2 == 0) {
                                fnNotify('Race is required in Parent2!');
                                document.getElementById('<%=ddlEthnicity2.ClientID %>').focus();
                                return false;
                            }
                            if (Relationship2 == 0) {
                                fnNotify('Relationship is required in Parent2!');
                                document.getElementById('<%=ddlRelationship2.ClientID %>').focus();
                                return false;
                            }
                        }

                        //Check whether user enter parent1 and parent2 details same or not
                        if (lname1.trim() != "" && fname1.trim() != "" && lname2.trim() != "" && fname2.trim() != "" && Relationship1 != 0 && Relationship2 != 0) {
                            var family_member1 = lname1 + "," + fname1;
                            var family_member2 = lname2 + "," + fname2;
                            var memberrole1 = member_role1.options[member_role1.selectedIndex].value;
                            var memberrole2 = member_role2.options[member_role2.selectedIndex].value;
                            if (family_member1 == family_member2 && memberrole1 == memberrole2) {
                                fnNotify('Parent(s)/Guardian(s) name and relationship should not be same!');
                                return false;
                            }
                            else {
                                if (phonezipcodeValidation()) {
                                    if (SchoolValidation()) {
                                        return true;
                                    }
                                    else {
                                        return false;
                                    }
                                } else {
                                    return false;
                                }
                            }
                        }
                        else {
                            if (phonezipcodeValidation()) {
                                if (SchoolValidation()) {
                                    return true;
                                }
                                else {
                                    return false;
                                }
                            } else {
                                return false;
                            }
                        }
                    }
                return true;
            }
            else {
                return false;
            }
        }
        else {
            return false;
        }

    }
    function SchoolValidation() {
        var cbSchoolAvailable = document.getElementById('<%=cbSchoolAvailable.ClientID %>');
        var cbEnrolledInSchool = document.getElementById('<%=cbEnrolledInSchool.ClientID %>');
        var txtSchoolStartDate = document.getElementById('<%=txtSchoolStartDate.ClientID %>');
        var txtRefSchoolName = document.getElementById('<%=txtRefSchoolName.ClientID %>');
        var ddlCurrentGrade = document.getElementById('<%=ddlCurrentGrade.ClientID %>');
        var ddlSchoolYear = document.getElementById('<%=ddlSchoolYear.ClientID %>');
        if (cbEnrolledInSchool.checked && cbSchoolAvailable.checked) {
            if (txtSchoolStartDate.value != "__/__/____" && txtRefSchoolName.value != "" && ddlCurrentGrade.selectedIndex != 0 && ddlSchoolYear.selectedIndex != 0) {
                if (chkSpecialEducation.checked && ddlSchoolType.selectedIndex != 0) {
                    fnNotify('Please fill the  	School Placement Type detail!');
                    return false;
                }
                else {
                    return true;
                }
            }
            else {
                fnNotify('Please fill the School details!');
                return false;
            }

        }
        else {
            return true;
        }
    }

    function phonezipcodeValidation() {
        if (Check_PhoneAndFaxNo(document.getElementById('<%=txtDJSDSSPhone.ClientID %>')) == false) {
                return false;
            }
            else {
                if (check_zipcodeNo(document.getElementById('<%=txtZip11.ClientID %>')) == false) {
                    return false;
                }
                else {
                    if (Check_PhoneAndFaxNo(document.getElementById('<%=txtWorkPhone.ClientID %>')) == false) {
                        return false;
                    }
                    else {
                        if (Check_PhoneAndFaxNo(document.getElementById('<%=txtContactPhone1.ClientID %>')) == false) {
                            return false;
                        }
                        else {
                            if (check_zipcodeNo(document.getElementById('<%=txtZip21.ClientID %>')) == false) {
                                return false;
                            }
                            else {
                                if (Check_PhoneAndFaxNo(document.getElementById('<%=txtWorkPhone2.ClientID %>')) == false) {
                                    return false;
                                }
                                else {
                                    if (Check_PhoneAndFaxNo(document.getElementById('<%=txtContactPhone2.ClientID %>')) == false) {
                                        return false;
                                    }
                                    if (!ValidatePhoneType()) {
                                        return false;
                                    } else {
                                        return true;
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }


        function VaildateTeamMemberDetails() {
            var rblTeamMemberType = document.getElementById('<%=rbtnlstTeam.ClientID %>');
            var rbTeamItem = rblTeamMemberType.getElementsByTagName('input');
            var isValidate = false;
            for (var i = 0; i <= rbTeamItem.length; i++) {
                if (rbTeamItem[i].checked) {
                    if (rbTeamItem[i].value == 'User') {
                        if (Page_ClientValidate("TeamMemSave")) {
                            isValidate = true;
                            break;
                        }
                    }
                    else if (rbTeamItem[i].value == 'Organization') {
                        if (Page_ClientValidate("TeamMemSave")) {
                            isValidate = true;
                            break;
                        }
                    }
                    else if (rbTeamItem[i].value == 'Family') {
                        isValidate = ValidationForFamilyDetails();
                        break;
                    }
                }
            }
            if (isValidate == true) {
                isValidate = CheckAddedDate();
            }
            return isValidate;
        }


        function ValidationForFamilyDetails() {
            var IsFamilyInfo = false;
            var ddlFamily = document.getElementById('<%=ddlFamily.ClientID %>').selectedIndex;
            var txtDateAdd = document.getElementById('<%=txtStartDateAdd.ClientID %>');
            if (ddlFamily == 0) {
                fnNotify('Team Member is required');
                IsFamilyInfo = false;
            }
            else if (txtDateAdd.value == '__/__/____') {
                fnNotify('Start date is required');
                IsFamilyInfo = false;
            }
            else {
                IsFamilyInfo = true;
            }
            return IsFamilyInfo;

        }

        function CheckFamilyEndDate() {
            var enddate_ok = false;
            var start_date = document.getElementById('<%=txtFmStartDate.ClientID %>');
            var End_date = document.getElementById('<%=txtFMEndDate.ClientID %>');
            var todaydate = new Date();
            todaydate.setHours(0, 0, 0, 0);
            if (Page_ClientValidate("FamilyDate")) {
                if (End_date.value != '__/__/____' && End_date != '') {
                    if (start_date.value != '__/__/____' && start_date != '') {
                        if (new Date(End_date.value) <= new Date(start_date.value)) {
                            enddate_ok = false;
                            fnNotify('End date should not be lesser or equal to start date!');
                        }
                        else if (new Date(End_date.value) > todaydate) {
                            enddate_ok = false;
                            fnNotify('End date should be lesser or equal to today\'s date!');
                        }
                        else {
                            enddate_ok = true;
                        }
                    }
                }
                else {
                    enddate_ok = true;
                }
            }
            if (enddate_ok) {
                enddate_ok = checkActivePOC("FamliyMember");
                if (!enddate_ok) {
                    $find('mpeFamilyMemberEdit').hide();
                }
            }
            return enddate_ok;
        }

        function CheckTeamEditDetails() {
            var validation_ok = false;
            var start_date = document.getElementById('<%=txtTMStartDate.ClientID %>');
            var End_date = document.getElementById('<%=txtTMEndDate.ClientID %>');
            var IsFacilitator = document.getElementById('<%=hdIsFacilitator.ClientID %>');
            if (IsFacilitator.value == "True") {
                if (start_date.value == '__/__/____') {
                    fnNotify('Start Date is required!')
                    validation_ok = false;
                }
                else {
                    validation_ok = CheckTeamEndDate();
                }
            }
            else {
                if (Page_ClientValidate("TeamEdit")) {
                    validation_ok = CheckTeamEndDate();
                }
            }
            if (validation_ok) {
                validation_ok = checkActivePOC("TeamMember");
                if (!validation_ok) {
                    $find('mpeTeamMemedit').hide();

                }
            }
            return validation_ok;
        }


        function CheckTeamEndDate() {
            var enddate_ok = false;
            var start_date = document.getElementById('<%=txtTMStartDate.ClientID %>');
            var End_date = document.getElementById('<%=txtTMEndDate.ClientID %>');
            var todaydate = new Date();
            todaydate.setHours(0, 0, 0, 0);
            if (End_date.value != '__/__/____' && End_date != '') {
                if (new Date(start_date.value) >= new Date(End_date.value)) {
                    enddate_ok = false;
                    fnNotify('End date should not be lesser or equal to start date!');
                }
                else if (new Date(End_date.value) > todaydate) {
                    enddate_ok = false;
                    fnNotify('End date should be lesser or equal to today\'s date!');
                }
                else {
                    enddate_ok = true;
                }
            }
            else {
                enddate_ok = true;
            }
            return enddate_ok;
        }


        function NavigateReportPage(ReportName) {
            var youthId = '<%= Request.QueryString["YID"] %>';
            var pageURL = '', width = 800, height = 600;
            var title = '';
            var Assesment = 'Report';
            switch (ReportName) {
                case 'Enrollment':
                    pageURL = 'ViewFacilitatorReport.aspx?y=' + youthId;
                    title = 'Enrollment Report';
                    break;
                case 'RO':
                    pageURL = '../Print/ViewResidentialOutcome.aspx?YID=' + youthId;
                    title = 'Residential Outcomes Report';
                    break;
                case 'YS':
                    pageURL = '../Print/YouthConnections_Print.aspx?YID=' + youthId;
                    title= 'Youth Support Report';
                    break;
                case 'FCS':
                    pageURL = '../Print/FamilyConnections_Print.aspx?YID=' + youthId;
                    title = 'Family Support Report';
                    break;
                default:
                    pageURL = 'ViewIndividualAssessmentReport.aspx?YId=' + youthId + "&AN=" + ReportName + "&FT=P" + "&S=All&IR=False";
                    switch (ReportName) {
                        case 'YN':
                            title = 'Youth Needs And Goals Report';
                            break;
                        case 'FST':
                            title = 'Family Satisfaction Report';
                            break;
                        case 'SO':
                            title = 'School Outcomes Report';
                            break;
                        case 'CO':
                            title = 'Community Outcomes Report';
                            break;
                        default:
                            title = "Report";
                            break;
                    }
                    break;
            }
            setTargetFrame(pageURL, width, height, title);
            return false;
        }

        function NavigateMenu(text) {
            var IsHealthAvailable = document.getElementById('hfIsHealthAvailable').value;
            var Navigate = '';
            var NavigateAss = '';
            var NavigateForm = '';
            var sText = text.split('~');
            Navigate = sText[0];

            switch (Navigate) {
                case 'FC':
                    NavigateAss = sText[1];
                    switch (NavigateAss) {
                        //Demograhics Form Check       
                        case 'DF':
                            var container = $find('<%= tcEnrollment.ClientID %>');
                            container.set_activeTabIndex(1);
                            if (container != null) {
                                var container_child = $find('<%= tcYouthDemographics.ClientID %>');
                                container_child.set_activeTabIndex(0);
                            }
                            break;
                            //School Record Check       
                        case 'SR':
                            var container = $find('<%= tcEnrollment.ClientID %>');
                            container.set_activeTabIndex(1);
                            if (container != null) {
                                var container_child = $find('<%= tcYouthDemographics.ClientID %>');
                                if (IsHealthAvailable == "Yes") {
                                    container_child.set_activeTabIndex(2);
                                } else {
                                    container_child.set_activeTabIndex(1);
                                }
                            }
                            break;
                            //Language and Insurance Check       
                        case 'LI':
                            var container = $find('<%= tcEnrollment.ClientID %>');
                            container.set_activeTabIndex(1);
                            if (container != null) {
                                var container_child = $find('<%= tcYouthDemographics.ClientID %>');
                                if (IsHealthAvailable == "Yes") {
                                    container_child.set_activeTabIndex(3);
                                } else {
                                    container_child.set_activeTabIndex(2);
                                }
                            }
                            break;
                            //Custody Record Check       
                        case 'CR':
                            var container = $find('<%= tcEnrollment.ClientID %>');
                            container.set_activeTabIndex(1);
                            if (container != null) {
                                var container_child = $find('<%= tcYouthDemographics.ClientID %>');
                                if (IsHealthAvailable == "Yes") {
                                    container_child.set_activeTabIndex(4);
                                } else {
                                    container_child.set_activeTabIndex(3);
                                }
                            }
                            break;
                            //Refferal Form Check       
                        case 'RF':
                            var container = $find('<%= tcEnrollment.ClientID %>');
                            container.set_activeTabIndex(1);
                            if (container != null) {
                                var container_child = $find('<%= tcYouthDemographics.ClientID %>');
                                if (IsHealthAvailable == "Yes") {
                                    container_child.set_activeTabIndex(5);
                                } else {
                                    container_child.set_activeTabIndex(4);
                                }
                            }
                            break;
                            //Assessment Form Check       
                        case 'AF':
                            NavigateForm = sText[2];
                            switch (NavigateForm) {
                                case 'C':
                                    var container = $find('<%= tcEnrollment.ClientID %>');
                                    container.set_activeTabIndex(4);
                                    if (container != null) {
                                        var container_child = $find('<%= tcAssessments.ClientID %>');
                                        container_child.set_activeTabIndex(0);
                                    }
                                    break;
                                case 'A':
                                    var container = $find('<%= tcEnrollment.ClientID %>');
                                    container.set_activeTabIndex(4);
                                    if (container != null) {
                                        var container_child = $find('<%= tcAssessments.ClientID %>');
                                        container_child.set_activeTabIndex(1);
                                    }
                                    break;
                                case 'P':
                                    var container = $find('<%= tcEnrollment.ClientID %>');
                                    container.set_activeTabIndex(4);
                                    if (container != null) {
                                        var container_child = $find('<%= tcAssessments.ClientID %>');
                                        container_child.set_activeTabIndex(2);
                                    }
                                    break;
                                case 'F':
                                    var container = $find('<%= tcEnrollment.ClientID %>');
                                    container.set_activeTabIndex(4);
                                    if (container != null) {
                                        var container_child = $find('<%= tcAssessments.ClientID %>');
                                        container_child.set_activeTabIndex(3);
                                    }
                                    break;
                            }
                            break;
                        case 'Ser': //Services Check
                            var container = $find('<%= tcEnrollment.ClientID %>');
                            container.set_activeTabIndex(5);
                            break;
                            //Poc Check        
                        case 'POC':
                            var container = $find('<%= tcEnrollment.ClientID %>');
                            container.set_activeTabIndex(5);
                            break;
                            //Enrollment Record        
                        case 'ER':
                            var container = $find('<%= tcEnrollment.ClientID %>');
                            container.set_activeTabIndex(1);
                            if (container != null) {
                                var container_child = $find('<%= tcYouthDemographics.ClientID %>');
                                container_child.set_activeTabIndex(0);
                            }
                            break;
                    }
                    break;
                    //Data check   
                case 'DC':
                    var container = $find('<%= tcEnrollment.ClientID %>');
                    container.set_activeTabIndex(1);
                    if (container != null) {
                        var container_child = $find('<%= tcYouthDemographics.ClientID %>');
                        container_child.set_activeTabIndex(0);
                    }
                    break;
                    //Family Member Check   
                case 'FM':
                    var container = $find('<%= tcEnrollment.ClientID %>');
                    container.set_activeTabIndex(2);
                    break;
                    //Team Member Check  
                case 'TM':
                    NavigateAss = sText[1];
                    switch (NavigateAss) {
                        //Assigned Facilitator      
                        case 'F':
                            var p = document.getElementById("<%=hfFacilitatorPermission.ClientID %>").value.trim();
                            if (document.getElementById('<%=lblFacilitator.ClientID %>').innerHTML == "--") {
                                if (p == "1") {
                                    window.location = "PendingEnrollments.aspx";
                                }
                                else if (p == "0") {
                                    fnNotify("You do not have permission to Assign Facilitator!");
                                }
                            }
                            else {
                                var container = $find('<%= tcEnrollment.ClientID %>');
                                container.set_activeTabIndex(1);
                                if (container != null) {
                                    var container_child = $find('<%= tcYouthDemographics.ClientID %>');
                                    container_child.set_activeTabIndex(0);
                                }
                            }
                            break;
                            //User,Non-User,Family Members Check      
                        default:
                            var container = $find('<%= tcEnrollment.ClientID %>');
                            container.set_activeTabIndex(3);
                            if (container != null) {
                                var container_child = $find('<%= tcTeamDetails.ClientID %>');
                                container_child.set_activeTabIndex(0);
                            }
                            break;
                    }
                    break;
                    //Team Meeting Check   
                case 'ME':
                    var container = $find('<%= tcEnrollment.ClientID %>');
                    container.set_activeTabIndex(3);
                    if (container != null) {
                        var container_child = $find('<%= tcTeamDetails.ClientID %>');
                        container_child.set_activeTabIndex(1);
                    }
                    break;
            }
            return false;
        }
        function CloseMailSend() {
            $find('mpeSenddocMail').hide();
            return false;
        }
        function EnrollmentDeleteConfirmation(statusHistoryId, deleteOption, isSingleCycle) {
            var hdnStatusHistoryId = document.getElementById('<%=hdnStatusHistoryId.ClientID %>');
            var hdnSingleCycle = document.getElementById('<%=hdnSingleCycle.ClientID %>');
            hdnStatusHistoryId.value = statusHistoryId;
            if (deleteOption == "SingleStatus") { $find('mpeDeleteSingleStatus').show(); }
            else {
                $find('mpeDeleteEnrollmentCycle').show();
                if (isSingleCycle != "") {
                    hdnSingleCycle.value = "True";
                }
            }
            return false;
        }
        function EnrollmentCycleDeleteCancelMsg() {
            $find('mpeDeleteEnrollmentCycle').hide();
            $find('mpeStatusHistory').show();
            fnNotify('Delete Action Cancelled by User!');
            return false;
        }

        function EnrollmentRecentStatusDelete(statusHistoryId, deleteOption, isSingleCycle) {
            var hdnStatusHistoryId = document.getElementById('<%=hdnStatusHistoryId.ClientID %>');
            var lbDeleteEnrollmentCycle = document.getElementById('<%=lbDeleteEnrollmentCycle.ClientID %>');
            hdnStatusHistoryId.value = statusHistoryId;
            lbDeleteEnrollmentCycle.setAttribute("onclick", "$find('mpeDeleteRecentStatus').hide();return EnrollmentDeleteConfirmation('" + statusHistoryId + "','" + deleteOption + "','" + isSingleCycle + "');");
            $find('mpeDeleteRecentStatus').show();
            return false;
        }
        //Export Youth Demographics and Assessments related details before deleting the specific 'Enrollment Cycle'.
        function ExportDemographicsAndAssessment() {
            //To Export Demographics related details 
            var iframe1 = document.createElement('iframe');
            iframe1.src = 'GenerateSpreadSheet.aspx?D=DE';
            iframe1.style.display = 'none';
            document.body.appendChild(iframe1);
            //To Export Core & Licensed Assessments
            var iframe2 = document.createElement('iframe');
            iframe2.src = 'GenerateSpreadSheet.aspx?D=EP';
            iframe2.style.display = 'none';
            document.body.appendChild(iframe2);
        }
        function setEnrollmentStatus() {
            var ddlEnrollmentStatus = document.getElementById('<%= ddlEnrollmentStatus.ClientID %>');
            if (ddlEnrollmentStatus != null && ddlEnrollmentStatus != undefined) {
                var enrollmentStatusValue = ddlEnrollmentStatus.options[ddlEnrollmentStatus.selectedIndex].value;
                var hdnEnrollmentStatusChanged = document.getElementById('<%=hdnEnrollmentStatusChanged.ClientID %>');
                hdnEnrollmentStatusChanged.value = enrollmentStatusValue;
            }
            return true;
        }
        function ShowMedicationPopup() {
            var hdnYouthIDValue = document.getElementById('<%= hdnYouthId.ClientID %>').value.trim();
            var frm = document.getElementById('<%= frmMedications.ClientID %>');
            frm.setAttribute("src", "");
            if (hdnYouthIDValue != "") {
                var url = "AddEditMedicationDetails.aspx?YId=" + hdnYouthIDValue;
                frm.setAttribute("src", url);
                $find("bmpeMedication").show();
            }
            return false;
        }

        function ClearPopupFrame() {
            var frame = document.getElementById('<%= frmMedications.ClientID %>');
            var commonframe = document.getElementById('<%= ifrmCommonPopup.ClientID %>');
            var hfPopupUnregister = document.getElementById("hfPopupUnregister");
            var url = commonframe.getAttribute("src");
            hfPopupUnregister.value = 'Yes';
            if (frame != null) {
                frame.setAttribute("src", "")
                frame.innerHTML = "";
            }
            if (commonframe != null) {
                commonframe.setAttribute("src", "")
                commonframe.innerHTML = "";
            }
            if (url.indexOf('ViewTeamMeetingReport') < 0) {
                RebindAllAssesmentGrid();
            }
        }

        function ReloadSource(reload, options) { //This argument is not necessary, but just in case!            
            if (reload) {
                var frame = document.getElementById('ifrmCommonPopup');
                var url = frame.getAttribute("src");
                frame.setAttribute("src", "");
                frame.setAttribute("src", url + (options != null ? options : ""));
            }
            return false;
        };


        function YouthMedications() {
            var ifReferralForm = document.getElementById('<%=ifReferralForm.ClientID %>');
            var isrc = ifReferralForm.src;
            ifReferralForm.src = '';
            ifReferralForm.innerHTML = '';
            ifReferralForm.src = isrc;
            $find('bmpeMedication').hide();
        }

        function SchoolDetailsPgLoad() {
            var pnlSchoolPlacement = document.getElementById("<%=pnlSchoolPlacement.ClientID%>")
            var cbSchoolAvailable = document.getElementById('<%=cbSchoolAvailable.ClientID %>');
            var cbEnrolledInSchool = document.getElementById('<%=cbEnrolledInSchool.ClientID %>');
            var txtSchoolStartDate = document.getElementById('<%=txtSchoolStartDate.ClientID %>');
            var txtRefSchoolName = document.getElementById('<%=txtRefSchoolName.ClientID %>');
            var ddlCurrentGrade = document.getElementById('<%=ddlCurrentGrade.ClientID %>');
            var chkbGeneralEducation = document.getElementById('<%=chkbGeneralEducation.ClientID %>');
            var chkSpecialEducation = document.getElementById('<%=chkSpecialEducation.ClientID %>');
            var ddlSchoolType = document.getElementById('<%=ddlSchoolType.ClientID %>');
            var ddlSchoolYear = document.getElementById('<%=ddlSchoolYear.ClientID %>');
            var hfReferralSchoolCheck = document.getElementById("<%=hfReferralSchoolCheck.ClientID%>")
            var hfSchoolID = document.getElementById("<%=hfSchoolID.ClientID%>")
            if (cbEnrolledInSchool.checked && cbSchoolAvailable.checked) {
                hfReferralSchoolCheck.value = "1";
                txtSchoolStartDate.disabled = false;
                txtRefSchoolName.disabled = false;
                ddlCurrentGrade.disabled = false;
                ddlSchoolYear.disabled = false;
                chkbGeneralEducation.disabled = false;
                chkSpecialEducation.disabled = false;
                if (chkSpecialEducation.checked) {
                    ddlSchoolType.disabled = false;
                }
                else {
                    ddlSchoolType.disabled = true;
                    ddlSchoolType.selectedIndex = 0;
                }
                pnlSchoolPlacement.style.backgroundColor = "white";
            } else {
                txtSchoolStartDate.disabled = true;
                txtRefSchoolName.disabled = true;
                ddlCurrentGrade.disabled = true;
                ddlSchoolYear.disabled = true;
                chkbGeneralEducation.disabled = true;
                chkSpecialEducation.disabled = true;
                ddlSchoolType.disabled = true;
                chkbGeneralEducation.checked = false;
                chkSpecialEducation.checked = false;
                txtSchoolStartDate.value = "__/__/____";
                txtRefSchoolName.value = "";
                hfReferralSchoolCheck.value = "";
                hfSchoolID.value = "";
                ddlCurrentGrade.selectedIndex = 0
                ddlSchoolYear.selectedIndex = 0;
                chkbGeneralEducation.checked = false;
                chkSpecialEducation.checked = false;
                ddlSchoolType.selectedIndex = 0;
                pnlSchoolPlacement.style.backgroundColor = "#dbdbdb";
            }
        }
        function EnableSchoolPlacement() {
            var pnlSchoolPlacement = document.getElementById("<%=pnlSchoolPlacement.ClientID%>")
            var cbSchoolAvailable = document.getElementById('<%=cbSchoolAvailable.ClientID %>');
            var cbEnrolledInSchool = document.getElementById('<%=cbEnrolledInSchool.ClientID %>');
            var txtSchoolStartDate = document.getElementById('<%=txtSchoolStartDate.ClientID %>');
            var txtRefSchoolName = document.getElementById('<%=txtRefSchoolName.ClientID %>');
            var ddlCurrentGrade = document.getElementById('<%=ddlCurrentGrade.ClientID %>');
            var chkbGeneralEducation = document.getElementById('<%=chkbGeneralEducation.ClientID %>');
            var chkSpecialEducation = document.getElementById('<%=chkSpecialEducation.ClientID %>');
            var ddlSchoolType = document.getElementById('<%=ddlSchoolType.ClientID %>');
            var ddlSchoolYear = document.getElementById('<%=ddlSchoolYear.ClientID %>');
            var hfReferralSchoolCheck = document.getElementById("<%=hfReferralSchoolCheck.ClientID%>")
            if (cbEnrolledInSchool.checked && cbSchoolAvailable.checked) {
                txtSchoolStartDate.disabled = false;
                txtRefSchoolName.disabled = false;
                ddlCurrentGrade.disabled = false;
                ddlSchoolYear.disabled = false;
                chkbGeneralEducation.disabled = false;
                chkSpecialEducation.disabled = false;
                ddlSchoolType.disabled = true;
                pnlSchoolPlacement.style.backgroundColor = "white";
            } else {
                txtSchoolStartDate.disabled = true;
                txtRefSchoolName.disabled = true;
                ddlCurrentGrade.disabled = true;
                ddlSchoolYear.disabled = true;
                chkbGeneralEducation.disabled = true;
                chkSpecialEducation.disabled = true;
                ddlSchoolType.disabled = true;
                txtSchoolStartDate.value = "__/__/____";
                txtRefSchoolName.value = "";
                ddlCurrentGrade.selectedIndex = 0
                ddlSchoolYear.selectedIndex = 0;
                chkbGeneralEducation.checked = false;
                chkSpecialEducation.checked = false;
                ddlSchoolType.selectedIndex = 0;
                hfReferralSchoolCheck.value = "";
                if (cbSchoolAvailable.checked) {
                    cbSchoolAvailable.checked = cbEnrolledInSchool.checked;
                }
                pnlSchoolPlacement.style.backgroundColor = "#dbdbdb";
            }
            $find('mpeRemoveSchool').hide();
            return false;
        }
        function cbSchool(CheckBox) {
            var hfSchoolID = document.getElementById("<%=hfSchoolID.ClientID%>")
            var hfReferralSchoolCheck = document.getElementById("<%=hfReferralSchoolCheck.ClientID%>")
            var cbSchoolAvailable = document.getElementById('<%=cbSchoolAvailable.ClientID %>');
            var cbEnrolledInSchool = document.getElementById('<%=cbEnrolledInSchool.ClientID %>');
            if (hfReferralSchoolCheck.value == "") {
                EnableSchoolPlacement();
            }
            else {
                if (CheckBox == "cbEnrolledInSchool") {
                    if (cbEnrolledInSchool.checked == false && cbSchoolAvailable.checked == true) {
                        $find('mpeRemoveSchool').show();
                    }
                    else {
                        EnableSchoolPlacement();
                    }
                }
                else if (CheckBox == "cbSchoolAvailable") {
                    if (cbSchoolAvailable.checked == false) {
                        $find('mpeRemoveSchool').show();
                    }
                    else {
                        EnableSchoolPlacement();
                    }
                }
            }
        }
        function DisplaySchoolPalcementType(EducationType) {
            var chkbGeneralEducation = document.getElementById('<%=chkbGeneralEducation.ClientID %>');
            var chkSpecialEducation = document.getElementById('<%=chkSpecialEducation.ClientID %>');
            var ddlSchoolType = document.getElementById('<%=ddlSchoolType.ClientID %>');
            var lblReqSchoolType = document.getElementById('<%=lblReqSchoolType.ClientID %>');
            if (EducationType == 'SE') {
                chkbGeneralEducation.checked = false;
                if (chkSpecialEducation.checked) {
                    ddlSchoolType.disabled = false;
                    lblReqSchoolType.style.display = '';
                    ddlSchoolType.selectedIndex = 0;
                }
                else {
                    chkbGeneralEducation.checked = false;
                    chkSpecialEducation.checked = false;
                    ddlSchoolType.disabled = true;
                    lblReqSchoolType.style.display = 'none';
                    ddlSchoolType.selectedIndex = 0;
                }
            } else if (EducationType == 'GE') {
                if (chkbGeneralEducation.checked) {
                    chkSpecialEducation.checked = false;
                    ddlSchoolType.disabled = true;
                    lblReqSchoolType.style.display = 'none';
                    ddlSchoolType.selectedIndex = 0;
                }
                else {
                    chkbGeneralEducation.checked = false;
                    chkSpecialEducation.checked = false;
                    ddlSchoolType.disabled = true;
                    lblReqSchoolType.style.display = 'none';
                    ddlSchoolType.selectedIndex = 0;
                }
            }
        }
        function CancelActionClick() {
            var cbSchoolAvailable = document.getElementById('<%=cbSchoolAvailable.ClientID %>');
            var cbEnrolledInSchool = document.getElementById('<%=cbEnrolledInSchool.ClientID %>');
            $find('mpeRemoveSchool').hide();
            cbSchoolAvailable.checked = true;
            cbEnrolledInSchool.checked = true;
            return false;
        }

        function ShowDocument(obj) {
            var youthId = document.getElementById('<%= hdnYouthId.ClientID %>').value.trim();
            UpdateYouthLastEditedDate(youthId);
            var l = ((screen.width - 450) / 2);
            var t = ((screen.height - 100) / 2);
            window.open('Documents.aspx?docId=' + $(obj).attr('DocId'), '_blank', 'toolbar=0,location=0,menubar=0,resizable=0,width=450,height=100,scrollbars=1,left=' + l + ',top=' + t);
            return false;
        }

        function AddPOCDateCheck() {
            var txtPOCStartDate = document.getElementById('<%=txtPOCStartDate.ClientID %>');
            var todaydate = new Date();
            todaydate.setHours(0, 0, 0, 0);
            if (txtPOCStartDate.value != "__/__/____") {
                if (new Date(txtPOCStartDate.value) > todaydate) {
                    fnNotify('The Start Date of this Plan Of Care must be on or after the Youth\’s Pending Status Start Date, cannot overlap a previous Plan Of Care (if any), or be set in the future.');
                    return false;
                }
                else {
                    return true;
                }
            }
            else {
                fnNotify('Start Date is required!');
                return false;
            }
        }

        function ComposeMailDocument() {
            if (!$("#lbComposeMaiNpPermission").hasClass("disabled")) {
                var lbComposeMaiNpPermission = document.getElementById('<%=lbComposeMaiNpPermission.ClientID %>');
                if (lbComposeMaiNpPermission.className != "aspNetDisabled disabled button") {
                    $find('mpeNoPermission').show();
                } else {
                    $find('mpeNoPermission').hide();
                }
                return false;
            }
            else {
                return false;
            }
        }

        //For Team Tab
        function CheckTeamAddDetails() {
            var validation_ok = false;
            var start_date = document.getElementById('<%=txtStartDateAdd.ClientID %>');
            var End_date = document.getElementById('<%=txtEndDateAdd.ClientID %>');
            var selectedVal = "";
            var selected = $('#<%=rbtnlstTeam.ClientID %> input[type=radio]:checked');
            selectedVal = selected.val();
            if (selectedVal != "Family") {
                var ddlTeamMember = document.getElementById('<%=ddlTeamMember.ClientID %>');
                var ddlTeamMemberRole = document.getElementById('<%=ddlTeamMemberRole.ClientID %>');
                if (ddlTeamMember.selectedIndex != 0) {
                    if (ddlTeamMemberRole.selectedIndex != 0) {
                        if (start_date.value != "__/__/____") {
                            validation_ok = CheckTeamAddEndDate();
                        }
                        else {
                            fnNotify('Start Date is required!')
                            validation_ok = false;
                        }
                    }
                    else {
                        fnNotify('Select Team Member Role!')
                        validation_ok = false;
                    }
                }
                else {
                    fnNotify('Select Team Member Name!')
                    validation_ok = false;
                }
            } else {
                var ddlFamily = document.getElementById('<%=ddlFamily.ClientID %>');
                if (ddlFamily.selectedIndex != 0) {
                    if (start_date.value != "__/__/____") {
                        validation_ok = CheckTeamAddEndDate();
                    }
                    else {
                        fnNotify('Start Date is required!')
                        validation_ok = false;
                    }
                } else {
                    fnNotify('Select Team Member Name!')
                    validation_ok = false;
                }
            }

            if (validation_ok) {
                validation_ok = checkActivePOC("TeamMemberAdd");
                if (!validation_ok) {
                    $find('mpeTeam').hide();

                }
            }
            return validation_ok;
        }

        function CheckTeamAddEndDate() {
            var enddate_ok = false;
            var start_date = document.getElementById('<%=txtStartDateAdd.ClientID %>');
            var End_date = document.getElementById('<%=txtEndDateAdd.ClientID %>');
            var todaydate = new Date();
            todaydate.setHours(0, 0, 0, 0);
            if (new Date(start_date.value) > todaydate) {
                enddate_ok = false;
                fnNotify('Start Date cannot be a future date!');
            }
            else {
                if (End_date.value != '__/__/____' && End_date != '') {
                    if (new Date(start_date.value) >= new Date(End_date.value)) {
                        enddate_ok = false;
                        fnNotify('End date should not be lesser or equal to start date!');
                    }
                    else if (new Date(End_date.value) > todaydate) {
                        enddate_ok = false;
                        fnNotify('End date should be lesser or equal to today\'s date!');
                    }
                    else {
                        enddate_ok = true;
                    }
                }
                else {
                    enddate_ok = true;
                }
            }
            return enddate_ok;
        }


        function CheckActivePOCLanguage() {
            var txtPreferredLanuage = document.getElementById('<%=txtPreferredLanguage.ClientID %>').value;
            var txtOtherPreferredlanguage = document.getElementById('<%=txtOtherPreferredlanguage.ClientID %>');
            var isValid = false;
            if (txtPreferredLanuage == "Other" || txtPreferredLanuage == "other") {
                if (txtOtherPreferredlanguage.value == '') {
                    fnNotify('Please enter a response for other language!');
                    isValid = false;
                }
                else {
                    isValid = checkActivePOC("Language");
                }
            } else {
                isValid = checkActivePOC("Language");
            }

            return isValid;
        }

        function ValidateFamliyDetails() {
            var isValid = false;
            var FamilyWithPOC = document.getElementById('<%=hfFamilyWithPOC.ClientID %>').value;
            if (Page_ClientValidate("FamMemEdit")) {
                if (FamilyWithPOC == "Yes") {
                    isValid = checkActivePOC("Family");
                    if (!isValid) {
                        $find('mpeFamEdit').hide();
                    }
                } else {
                    isValid = true;
                }
            }
            else {
                isValid = false;
            }
            return isValid
        }

        function checkActivePOC(arg) {
            var hfHasActivePOC = document.getElementById('<%=hfHasActivePOC.ClientID %>');
            var hfAutomaticUpdatePOC = document.getElementById('<%=hfAutomaticUpdatePOC.ClientID %>');
            var hfEditPopupName = document.getElementById('<%=hfEditPopupName.ClientID %>');
            if (hfHasActivePOC != null && hfAutomaticUpdatePOC != null && hfHasActivePOC.value != '' && hfAutomaticUpdatePOC.value == "False") {
                hfEditPopupName.value = arg;
                if (arg != "Custody" && arg != "Family") {
                    OpenPopup('UpdatePOCConfirmPopup');
                }
                return false;
            }
            else {
                return true;
            }
        }

        function checkIsStatusUpdate(condition) {
            var hfEnrollmentStatusUpdateinPOC = document.getElementById('<%=hfEnrollmentStatusUpdateinPOC.ClientID %>');
            if (hfEnrollmentStatusUpdateinPOC.value != "") {
                hfEnrollmentStatusUpdateinPOC.value = condition;
            }
            ClosePopup('UpdatePOCConfirmPopupES');
            return false;
        }

        function CopyYouthPhone1(Condition) {
            //Youth Phone Number 1 & Its Type
            var hfYouthPhone1 = document.getElementById('<%=hfYouthPhone1.ClientID %>');
            var hfYouthPhoneType1 = document.getElementById('<%=hfYouthPhoneType1.ClientID %>');
            var txtYouthPhoneNo = document.getElementById('<%=txtYouthPhoneNo.ClientID %>');
            var ddlPhoneType1 = document.getElementById('<%=ddlYouthPhoneType1.ClientID %>');
            var cbIsYouthPhone1Parent1 = document.getElementById('<%=cbIsYouthPhone1Parent1.ClientID %>');
            var cbIsYouthPhone1Parent2 = document.getElementById('<%=cbIsYouthPhone1Parent2.ClientID %>');
            var cbIsYouthPhone1Family = document.getElementById('<%=cbIsYouthPhone1Family.ClientID %>');
            var cbIsYouthPhone1FamilyEdit = document.getElementById('<%=cbIsYouthPhone1FamilyEdit.ClientID %>');

            //Parent/Gaurdian 1 
            var txtWorkPhone = document.getElementById('<%=txtWorkPhone.ClientID %>');
            var ddlRefP1Type1 = document.getElementById('<%=ddlRefP1Type1.ClientID %>');

            //Parent/Gaurdian 2
            var txtWorkPhone2 = document.getElementById('<%=txtWorkPhone2.ClientID %>');
            var ddlRefP2Type1 = document.getElementById('<%=ddlRefP2Type1.ClientID %>');

            switch (Condition) {
                case "RefParent1":

                    //Parent/Gaurdian 1
                    if (cbIsYouthPhone1Parent1.checked) {
                        txtWorkPhone.value = txtYouthPhoneNo.value;
                        ddlRefP1Type1.selectedIndex = ddlPhoneType1.selectedIndex;
                    }
                    else {
                        txtWorkPhone.value = '(___) ___-____';
                        ddlRefP1Type1.selectedIndex = 0;
                    }
                    break;

                case "RefParent2":

                    //Parent/Gaurdian 2
                    if (cbIsYouthPhone1Parent2.checked) {
                        txtWorkPhone2.value = txtYouthPhoneNo.value;
                        ddlRefP2Type1.selectedIndex = ddlPhoneType1.selectedIndex;
                    }
                    else {
                        txtWorkPhone2.value = '(___) ___-____';
                        ddlRefP2Type1.selectedIndex = 0;
                    }
                    break;

                case "FamilyAdd":

                    //Family Tab Add
                    var txtContactPhone = document.getElementById('<%=txtContactPhone.ClientID %>');
                    var ddlPhoneType1Family = document.getElementById('<%=ddlPhoneType1.ClientID %>');

                    if (cbIsYouthPhone1Family != null && cbIsYouthPhone1Family.checked) {
                        txtContactPhone.value = hfYouthPhone1.value;
                        ddlPhoneType1Family.selectedIndex = 0; //Clear the PhoneNumber 1 Type
                        //Set the PhoneNumber 1 Type value
                        for (var i = 0; i < ddlPhoneType1Family.options.length; i++) {
                            if (ddlPhoneType1Family.options[i].value == hfYouthPhoneType1.value) {
                                ddlPhoneType1Family.options[i].selected = true;
                            }
                        }
                    }
                    else if (cbIsYouthPhone1Family != null) {
                        txtContactPhone.value = '(___) ___-____';
                        ddlPhoneType1Family.selectedIndex = 0;
                    }
                    break;

                case "FamilyEdit":

                    //Family Tab Edit
                    var txtContactPhoneEdit = document.getElementById('<%=txtContactPhoneEdit.ClientID %>');
                    var ddlPhoneType1Edit = document.getElementById('<%=ddlPhoneType1Edit.ClientID %>');

                    if (cbIsYouthPhone1FamilyEdit != null && cbIsYouthPhone1FamilyEdit.checked) {
                        txtContactPhoneEdit.value = hfYouthPhone1.value;
                        ddlPhoneType1Edit.selectedIndex = 0; //Clear the PhoneNumber 1 Type
                        //Set the PhoneNumber 1 Type value
                        for (var i = 0; i < ddlPhoneType1Edit.options.length; i++) {
                            if (ddlPhoneType1Edit.options[i].value == hfYouthPhoneType1.value) {
                                ddlPhoneType1Edit.options[i].selected = true;
                            }
                        }
                    }
                    else if (cbIsYouthPhone1FamilyEdit != null) {
                        txtContactPhoneEdit.value = '(___) ___-____';
                        ddlPhoneType1Edit.selectedIndex = 0;
                    }
                    break;

                case "YouthPhone":

                    //Parent/Gaurdian 1
                    if (cbIsYouthPhone1Parent1.checked) {
                        txtWorkPhone.value = txtYouthPhoneNo.value;
                        ddlRefP1Type1.selectedIndex = ddlPhoneType1.selectedIndex;
                    }
                    else {
                        txtWorkPhone.value = '(___) ___-____';
                        ddlRefP1Type1.selectedIndex = 0;
                    }

                    //Parent/Gaurdian 2
                    if (cbIsYouthPhone1Parent2.checked) {
                        txtWorkPhone2.value = txtYouthPhoneNo.value;
                        ddlRefP2Type1.selectedIndex = ddlPhoneType1.selectedIndex;
                    }
                    else {
                        txtWorkPhone2.value = '(___) ___-____';
                        ddlRefP2Type1.selectedIndex = 0;
                    }
                    break;
            }
            return true;
        }


        function ValidatePhoneType() {
            var ValidateType = true;
            var Ref1_Phone1 = document.getElementById('<%=txtWorkPhone.ClientID %>').value;
            var Ref1_PhoneType1 = document.getElementById('<%=ddlRefP1Type1.ClientID %>').selectedIndex;
            var Ref2_Phone1 = document.getElementById('<%=txtWorkPhone2.ClientID %>').value;
            var Ref2_PhoneType1 = document.getElementById('<%=ddlRefP2Type1.ClientID %>').selectedIndex;
            if (ValidateType) {
                if (Ref1_Phone1 != '(___) ___-____') {
                    if (Ref1_PhoneType1 == 0) {
                        fnNotify('Phone 1 Type is required!');
                        document.getElementById('<%=ddlRefP1Type1.ClientID %>').focus();
                        ValidateType = false;
                    }
                }
            }
            if (ValidateType) {
                if (Ref2_Phone1 != '(___) ___-____') {
                    if (Ref2_PhoneType1 == 0) {
                        fnNotify('Phone 1 Type is required!');
                        document.getElementById('<%=ddlRefP2Type1.ClientID %>').focus();
                        ValidateType = false;
                    }
                }
            }
            return ValidateType;
        }

        //check Phone Type by CustomValidator
        function checkPhoneType(sender, args) {
            var phone_ok = true;
            var phoneType = document.getElementById(sender.controltovalidate);
            var Phone1 = document.getElementById('<%=txtContactPhoneEdit.ClientID %>').value;
            if (Phone1 != '(___) ___-____' && phoneType.selectedIndex == 0) {
                phoneType.focus();
                phone_ok = false;
            }
            args.IsValid = phone_ok;
        }
        function CheckJurisdictionDetails(Selecteddropdown) {
            var ddlJurisdictionR = document.getElementById('<%=ddlJurisdictionR.ClientID %>');
            var ddlJurisdictionLeagal = document.getElementById('<%=ddlJurisdictionLeagal.ClientID %>');
            if (ddlJurisdictionR.value != '' && ddlJurisdictionR.value != '0') {
                if (ddlJurisdictionR.value == ddlJurisdictionLeagal.value) {
                    fnNotify("County/Jurisdiction of residence and County/Jurisdiction with legal and/or financial responsibility should not be same!");
                    $(Selecteddropdown).val('');
                }
            }
        }

        function TargetFrame(source, height, width, title, includeClose) {
            if ($find("mpeCommonPopup") != null) {
                $find("mpeCommonPopup").hide();
            }
            var wrapper = document.getElementById("pnlCommonPopup");
            var frame = document.getElementById("ifrmCommonPopup");
            var heading = document.getElementById("lblCommonPopupHeader");
            if (title === undefined) {
                title = '';
            }
            if (wrapper != null && frame != null && heading != null) {
                heading.innerHTML = title;
                if ($(window).width() <= 980) {
                    $(wrapper).css("height", height + 'px');
                    $(wrapper).css("width", '800px');
                }
                frame.setAttribute("height", 100 + '%');
                frame.setAttribute("width", 100 + '%')
                frame.setAttribute("src", source);
                if ($find("mpeCommonPopup") != null) {
                    $find("mpeCommonPopup").show();
                }
            }
            if (includeClose !== undefined) {
                frame.setAttribute("height", 94 + '%');
                $(".div-reports-top").removeAttr('style');
            }
            else {
                $(".div-reports-top").attr('style', 'display:none');
            }
            return false;
        }

        function FamilyHisFrame(source, height, width) {
            $find("mpeCommonPopup").hide();
            var youthName = document.getElementById('<%=lblNameDashboard.ClientID %>').innerHTML;
            var wrapper = document.getElementById("pnlCommonPopup");
            var frame = document.getElementById("ifrmCommonPopup");
            var heading = document.getElementById("lblCommonPopupHeader");
            if (wrapper != null && frame != null && heading != null) {
                frame.setAttribute("height", height + 'px');
                frame.setAttribute("width", 100 + '%');
                $(wrapper).css("height", height + 'px');
                $(wrapper).css("width", '700px');
                frame.setAttribute("src", source + "&yName=" + youthName);
                $find("mpeCommonPopup").show();
            }
            return false;
        }

        function PopupClose(PopupID) {
            var frame = document.getElementById("ifrmCommonPopup");
            var hfPopupUnregister = document.getElementById("hfPopupUnregister");
            if (frame != null) {
                frame.setAttribute("src", "")
                frame.innerHTML = "";
            }
            if ($find(PopupID) != null && $find(PopupID) !== undefined) {
                $find(PopupID).hide();
                hfPopupUnregister.value = 'Yes';
            }
            return false;
        }

        function CloseCommonPopupOnCondition() {
            var hfPopupUnregister = document.getElementById("hfPopupUnregister");
            if ($find('mpeCommonPopup') != null && hfPopupUnregister.value == 'Yes') {
                $find('mpeCommonPopup').hide();
            }
            return false;
        }

        function RedirectPopupDisplay() {
            setPopupWidthDynamicallyBasedonContent("ContactServiceNotes");
            var youthId = '<%= Request.QueryString["YID"] %>';
            TargetFrame('../Youth/POPUP_ContactServiceNote.aspx?YId=' + youthId, 555, 775, 'Youth Details(POC Form)');
            return false;

        }


        function showServiceNotes(lbServiceNotesEdit) {
            setPopupWidthDynamicallyBasedonContent("ContactServiceNotes");
            var youthId = '<%= Request.QueryString["YID"] %>';
            var ContactServiceNoteID = lbServiceNotesEdit.getAttribute('ContactServiceNoteID');
            TargetFrame('../Youth/POPUP_ContactServiceNote.aspx?YId=' + youthId + '&CSNID=' + ContactServiceNoteID, 555, 775, 'Youth Details(POC Form)');
            return false;
        }

        function ViewContactServicesReport(lnkPrintContactProgress) {
            var pageURL = '';
            var title = '';
            var youthId = '<%= Request.QueryString["YID"] %>';
            var ContactServiceNoteID = lnkPrintContactProgress.getAttribute('ContactServiceNoteID');
            pageURL = '../Youth/PrintServiceNoteReport.aspx?YId=' + youthId + '&CSNID=' + ContactServiceNoteID + '&r=Single';
            title = "View Contact Services Report";
            setTargetFrame(pageURL, 800, 600, title);
            return false;
        }

        function ContactServicesReport(object, Type) {
            if (!$(object).hasClass("aspNetDisabled")) {
                setPopupWidthDynamicallyBasedonContent("ContactServiceNotesReport");
                var youthId = '<%= Request.QueryString["YID"] %>';
                TargetFrame('../Youth/ViewContactServicesReport.aspx?YId=' + youthId + '&CSNID=0&r=' + Type, 500, 775, '');
                return false;
            }
        }


        function ReloadServiceNotesTab() {
            var button = document.getElementById('<%=lbServiceNotes.ClientID %>');
            CallServerSideEvent(button);
            window.parent.PopupClose('mpeCommonPopup');
            return false;
        }

        function AddCustodyDetails() {
            setPopupWidthDynamicallyBasedonContent("Custody");
            var mainScreenWidth = $(window).width();
            var youthId = '<%= Request.QueryString["YID"] %>';
            var CurrentDate = document.getElementById('<%=hfCurrentDate.ClientID%>').value;
            var gvCustodyDetails = document.getElementById('<%=grdCustodyDetails.ClientID%>').rows;
            var StartDate = "01/01/1900";
            var YouthName = document.getElementById('<%=lblFundingYouthName.ClientID%>').innerHTML;
            var dob = document.getElementById('txtDOB').value;
            if (dob == '__/__/____' || dob == '00/00/0000') {
                dob = "01/01/1900";
            }
            if (gvCustodyDetails != null && gvCustodyDetails.length > 1) {
                StartDate = gvCustodyDetails[1].cells[2].textContent;
            }
            if (StartDate == CurrentDate) {
                fnWarning('A new Custody Status record cannot be added! Time limit between any two Custody Status records is one day.');
            } else {
                if (mainScreenWidth >= 980) {
                    TargetFrame('../Youth/POPUP_CustodyDetails.aspx?YID=' + youthId + '&M=Add' + '&dob=' + dob + '&Yname=' + YouthName, 540, 680, 'Youth Details(Custody Details)');
                } else {
                    TargetFrame('../Youth/POPUP_CustodyDetails.aspx?YID=' + youthId + '&M=Add' + '&dob=' + dob + '&Yname=' + YouthName + '&CheckWidth=' + mainScreenWidth, 540, 680, 'Youth Details(Custody Details)'); //Pass window width for set popup width as 800px
                }
            }
            return false;
        }


        function EditYouthCustodyDetails(lnkCustodyEdit) {
            var mainScreenWidth = $(window).width();
            setPopupWidthDynamicallyBasedonContent("Custody");
            var youthId = '<%= Request.QueryString["YID"] %>';
            var YouthName = document.getElementById('<%=lblFundingYouthName.ClientID%>').innerHTML;
            var dob = document.getElementById('txtDOB').value;
            if (dob == '__/__/____' || dob == '00/00/0000') {
                dob = "01/01/1900";
            }
            var CustodyId = lnkCustodyEdit.getAttribute('CustodyId');
            var IsLinkedWithPOC = lnkCustodyEdit.getAttribute('IsLinkedWithPOC');
            if (mainScreenWidth >= 980) {
                TargetFrame('../Youth/POPUP_CustodyDetails.aspx?YID=' + youthId + '&M=Update' + '&dob=' + dob + '&CustodyId=' + CustodyId + '&IsLinkedWithPOC=' + IsLinkedWithPOC + '&Yname=' + YouthName, 540, 680, 'Youth Details(Custody Details)');
            } else {
                TargetFrame('../Youth/POPUP_CustodyDetails.aspx?YID=' + youthId + '&M=Update' + '&dob=' + dob + '&CustodyId=' + CustodyId + '&IsLinkedWithPOC=' + IsLinkedWithPOC + '&Yname=' + YouthName + '&CheckWidth=' + mainScreenWidth, 540, 680, 'Youth Details(Custody Details)'); //Pass window width for set popup width as 800px
            }
            return false;
        }

        function GetActivePOcId() {
            var hfHasActivePOC = document.getElementById('<%=hfHasActivePOC.ClientID %>').value;
            return hfHasActivePOC;
        }


        function ReloadCustodyTab() {
            PopupClose('mpeCommonPopup');
            var button = document.getElementById('<%=lbCustodyStatus.ClientID %>');
            CallServerSideEvent(button);
            return false;
        }
        function ShowIFramePopUp(form, mode, height, width, headerText, objLinkButton) {
            var youthId = '<%= Request.QueryString["YID"] %>';
            var mainScreenWidth = $(window).width();
            var YouthName = document.getElementById('<%=lblFundingYouthName.ClientID%>').innerHTML;

            switch (form) {
                case "Family":
                    setPopupWidthDynamicallyBasedonContent("FamilyAddEdit");
                    if (mode == "Add") {
                        TargetFrame('../Youth/POPUP_FamilyDetails.aspx?YId=' + youthId + "&Activity=PopupFamily&Yname=" + YouthName, height, width, headerText);
                    }
                    else {
                        var FamilyID = objLinkButton.getAttribute('FamilyID');
                        var IsLinkedWithPOC = objLinkButton.getAttribute('IsLinkedWithPOC');
                        TargetFrame('../Youth/POPUP_FamilyDetails.aspx?YId=' + youthId + '&Activity=PopupFamily&Type=Edit&FID=' + FamilyID + '&IsLinkedWithPOC=' + IsLinkedWithPOC + "&Yname=" + YouthName, height, width, headerText);
                    }
                    break;
                case "Diagnoses":
                    if (mainScreenWidth >= 980) {
                        TargetFrame('../Youth/AddEditDiagnosisDetails.aspx?YId=' + youthId + '&Activity=Diagnosis', height, width, '');
                    } else {
                        TargetFrame('../Youth/AddEditDiagnosisDetails.aspx?YId=' + youthId + '&Activity=Diagnosis&CheckWidth=' + mainScreenWidth, height, width, ''); //Pass window width for set popup width as 800px
                    }
                    break;
            }
            return false;
        }
        function BindFamilyDetails() {
            PopupClose('mpeCommonPopup');
            var button = document.getElementById('lbBindFamilyDetails');
            CallServerSideEvent(button);
        }
        function YouthDiagnosesPopupClose() {
            var ifReferralForm = document.getElementById('<%=ifReferralForm.ClientID %>');
            var isrc = ifReferralForm.src;
            ifReferralForm.src = '';
            ifReferralForm.innerHTML = '';
            ifReferralForm.src = isrc;
            PopupClose('mpeCommonPopup');
        }


        function DeleteServiceNotes(lbServiceNotesDelete) {
            var ContactServiceNoteID = lbServiceNotesDelete.getAttribute('ContactServiceNoteID');
            var IsBillableContact = lbServiceNotesDelete.getAttribute('IsBillableContact');
            document.getElementById('<%=hfContactServiceNoteID.ClientID %>').value = ContactServiceNoteID;
            if (IsBillableContact == "True") {
                $find("mpeBillableDelete").show();

            } else {
                $find("mpeContactProgDel").show();
            }
            return false;
        }

        function checkSignedDate(sender, args) {
            if (sender._selectedDate > new Date()) {
                fnNotify("Date Signed cannot be a future date!");
                sender._textbox.set_Value('__/__/____');
            }
        }

        function CheckDateSignedDate(DateSigned) {
            var ds_ok = false;
            if (checkValidDate(DateSigned, 'Consent for Fidelity Assessment Date')) {
                var dSigned = DateSigned.value;
                var current = new Date();
                var currentyear = current.getFullYear();
                var currentmonth = current.getMonth();
                currentmonth = currentmonth + 1;
                var currentday = current.getDate();
                var cmonth = '';
                var cday = '';
                if (String(currentmonth).length == 1) { cmonth = (new String('0') + new String(currentmonth)); }
                else { cmonth = new String(currentmonth); }
                if (String(currentday).length == 1) { cday = (new String('0') + new String(currentday)); }
                else { cday = new String(currentday); }
                var c_date = new String(new String(currentyear) + cmonth + cday);
                c_date = parseInt(c_date);
                var split_dob = dSigned.split("/");
                var e_date = new String(split_dob[2] + split_dob[0] + split_dob[1]);
                e_date = parseInt(e_date);
                if (e_date > c_date) {
                    fnNotify('Date Signed cannot be a future date!');
                    DateSigned.value = '__/__/____';
                    DateSigned.focus();
                    ds_ok = false;
                }
                else {
                    ds_ok = true;
                }
            }
            return ds_ok;
        }

