        function ReloadTeamMeetingAttendedMemberSig() {
            var button = document.getElementById('<%=lbReloadTeammenbersignGrid.ClientID %>');
            CallServerSideEvent(button);
            return false;
        }

        function getteamtype() {
            if (document.getElementById('<%=chkTeamMeetingReport.ClientID%>').checked == false && document.getElementById('<%=chkTeamMeetingsignatureReport.ClientID%>').checked == false) {
                fnNotify('Please Select Atleast one Includes Report Type!');
                return false;

            }

        }

