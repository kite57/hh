                function GoogleDocumentViewer(path) {
                    $('#embedURL-gdocsviewer').empty();
                    $("#embedURL").attr("href", path);
                    $('a.embed').gdocsViewer({ width: '100%', height: '100%' });
                    $('#embedURL').gdocsViewer();
                }
            </script>
