        /*Team Tab Activity*/
        function TeamTabChanged(sender, args) {
            var TabContainerId = sender.get_activeTab().get_id();
            var TabPanelId = TabContainerId.replace("MainContent_tcEnrollment_tpTeam_tcTeamDetails_", "");
            var tbname = '';
            if (TabPanelId == "tpTeamMembers") {
                document.getElementById('<%=hfTeamActivity.ClientID%>').value = "tpTeamMembers";
                tbname = document.getElementById('<%=hfTeamActivity.ClientID%>').value;
                CallTeamActivitySave(tbname);

            }
            else if (TabPanelId = "tpTeamMeetings") {
                document.getElementById('<%=hfTeamActivity.ClientID%>').value = "tpTeamMeetings";
                tbname = document.getElementById('<%=hfTeamActivity.ClientID%>').value;
                CallTeamActivitySave(tbname);
            }
    }

    function CallTeamActivitySave(tabname) {
        PageMethods.SaveTeamActivity(tabname, onSucess, onError);
        function onSucess(result) { }
        function onError(result) { }
    }

    /*Assessment Tab Activity*/
    function AssessmentTabChanged(sender, args) {

        var TabContainerId = sender.get_activeTab().get_id();
        var TabPanelId = TabContainerId.replace("MainContent_tcEnrollment_tpAssessments_tcAssessments_", "");
        var tbname = '';
        if (TabPanelId == "tpCoreAssessments") {
            document.getElementById('<%=hfAssessmentActicity.ClientID%>').value = "tpCoreAssessments";
            tbname = document.getElementById('<%=hfAssessmentActicity.ClientID%>').value;
            CallAssessmentActivitySave(tbname);
        }
        else if (TabPanelId == "tpPlanOfCareAssessment") {
            document.getElementById('<%=hfAssessmentActicity.ClientID%>').value = "tpPlanOfCareAssessment";
            tbname = document.getElementById('<%=hfAssessmentActicity.ClientID%>').value;
            CallAssessmentActivitySave(tbname);
        }
        else if (TabPanelId == "tpCustomAssessments") {
            document.getElementById('<%=hfAssessmentActicity.ClientID%>').value = "tpCustomAssessments";
            tbname = document.getElementById('<%=hfAssessmentActicity.ClientID%>').value;
            CallAssessmentActivitySave(tbname);
        }
        else if (TabPanelId == "tpFamilyTimeLine") {
            document.getElementById('<%=hfAssessmentActicity.ClientID%>').value = "tpFamilyTimeLine";
                tbname = document.getElementById('<%=hfAssessmentActicity.ClientID%>').value;
                CallAssessmentActivitySave(tbname);
            }
            }

function CallAssessmentActivitySave(tabname) {
    PageMethods.SaveAssessmentActivity(tabname, onSucess, onError);
    function onSucess(result) { }
    function onError(result) { }
}
        /*To Show Action Needed PopUp*/
function ShowActionNeededPopup() {
    $find('bmpeActionNeeded').show();
    return false;
}
        /*To Hide Action Needed*/
function HideActionNeeded() {
    $find('bmpeActionNeeded').hide();
    var ActionNeededClassName = document.getElementsByClassName("ActionNeeded");
    for (var i = 0; i < ActionNeededClassName.length; i++) {
        ActionNeededClassName[i].style.display = "none";
    }
        }
