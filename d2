    <script type="text/javascript" language="javascript ">
        var myfn;
        function clearservice() {
            $find("bmpeAddServices_ModalPopupExtender").hide();
            return false;
        }

        function RebindYouthReferralDetails(MsgType) {
            var button = document.getElementById('<%=lbReferralRebind.ClientID %>');
            var hCustodyMsgType = document.getElementById('<%=hCustodyMsgType.ClientID %>');
            hCustodyMsgType.value = MsgType;
            CallServerSideEvent(button);
        }

        function onListPopulated() {
            var completionList = $find('<%=aceSearchName.ClientID%>').get_completionList();
            completionList.style.width = 'auto';
        }

        function SetReferralIfram() {
            var frame = document.getElementById("ifReferralForm");
            var YouthID = document.getElementById('<%=hdnYouthId.ClientID %>').value
            var source = "../Youth_Enrollment/YouthReferralDetails.aspx?YID=" + YouthID + "&Activity=YouthReferral";
            frame.setAttribute("src", source);
        }

        function YouthDemographicsTabChane(sender, args) {
            /* added by Anubaharathi to Save activity Monitor*/
            var TabContainerId = sender.get_activeTab().get_id();
            var TabPanelId = TabContainerId.replace("MainContent_tcEnrollment_tpYouth_tcYouthDemographics_", "");
            var tbname = '';
            if (TabPanelId == "tpDemographics") {
                document.getElementById('<%=hfDemographicsActivity.ClientID%>').value = "tpDemographics";
                tbname = document.getElementById('<%=hfDemographicsActivity.ClientID%>').value;
                CallDemographicsActivity(tbname)
            }
            else if (TabPanelId == "tpHealth") {
                document.getElementById('<%=hfDemographicsActivity.ClientID%>').value = "tpHealth";
                tbname = document.getElementById('<%=hfDemographicsActivity.ClientID%>').value;
                CallDemographicsActivity(tbname)
            }
            else if (TabPanelId == "tpSchool") {
                document.getElementById('<%=hfDemographicsActivity.ClientID%>').value = "tpSchool";
                tbname = document.getElementById('<%=hfDemographicsActivity.ClientID%>').value;
                CallDemographicsActivity(tbname)
            }
            else if (TabPanelId == "tpLanguage") {
                document.getElementById('<%=hfDemographicsActivity.ClientID%>').value = "tpLanguage";
                tbname = document.getElementById('<%=hfDemographicsActivity.ClientID%>').value;
                CallDemographicsActivity(tbname)
            }
            else if (TabPanelId == "tpCustody") {
                document.getElementById('<%=hfDemographicsActivity.ClientID%>').value = "tpCustody";
                tbname = document.getElementById('<%=hfDemographicsActivity.ClientID%>').value;
                CallDemographicsActivity(tbname)
            }
            else if (TabPanelId == "tpReferralForm") {
                document.getElementById('<%=hfDemographicsActivity.ClientID%>').value = "tpReferralForm";
                tbname = document.getElementById('<%=hfDemographicsActivity.ClientID%>').value;
                CallDemographicsActivity(tbname)
            }
            else if (TabPanelId == "tpWrapTrack") {
                document.getElementById('<%=hfDemographicsActivity.ClientID%>').value = "tpWrapTrack";
                tbname = document.getElementById('<%=hfDemographicsActivity.ClientID%>').value;
                CallDemographicsActivity(tbname)
            }
            /* End */
            var tabename = sender.get_activeTabIndex();
            var IsHealthAvailable = document.getElementById('hfIsHealthAvailable').value;
            if (IsHealthAvailable == "Yes") {
                if (tabename == 5) {
                    SetReferralIfram();
                }
                else if (tabename == 1) {
                    SetHealthTabIframe();
                }
            } else {
                if (tabename == 4) {
                    SetReferralIfram();
                }
            }
            return false;
        }

        function YouthEnrollmentTabChane(sender, args) {
    /* added by Anubaharathi to Save activity Monitor*/
    var TabContainerId = sender.get_activeTab().get_id();
    var TabPanelId = TabContainerId.replace("MainContent_tcEnrollment_", "");
    var tbname = '';
    if (TabPanelId == "tpDashboard") {
        document.getElementById('<%=hfYouthActivity.ClientID%>').value = "tpDashboard";
        tbname = document.getElementById('<%=hfYouthActivity.ClientID%>').value;
        CallActivity(tbname)
    }
    else if (TabPanelId == "tpYouth") {
        document.getElementById('<%=hfYouthActivity.ClientID%>').value = "tpYouth";
        tbname = document.getElementById('<%=hfYouthActivity.ClientID%>').value;
        CallActivity(tbname)
    }
    else if (TabPanelId == "tpFamily") {
        document.getElementById('<%=hfYouthActivity.ClientID%>').value = "tpFamily";
        tbname = document.getElementById('<%=hfYouthActivity.ClientID%>').value;
        CallActivity(tbname)
    }
    else if (TabPanelId == "tpTeam") {
        document.getElementById('<%=hfYouthActivity.ClientID%>').value = "tpTeam";
                tbname = document.getElementById('<%=hfYouthActivity.ClientID%>').value;
                CallActivity(tbname)
            }
            else if (TabPanelId == "tpAssessments") {
                document.getElementById('<%=hfYouthActivity.ClientID%>').value = "tpAssessments";
                tbname = document.getElementById('<%=hfYouthActivity.ClientID%>').value;
                CallActivity(tbname)
            }
            else if (TabPanelId == "tpServices") {
                document.getElementById('<%=hfYouthActivity.ClientID%>').value = "tpServices";
                tbname = document.getElementById('<%=hfYouthActivity.ClientID%>').value;
                CallActivity(tbname)
            }
            else if (TabPanelId == "tpPoc") {
                document.getElementById('<%=hfYouthActivity.ClientID%>').value = "tpPoc";
                tbname = document.getElementById('<%=hfYouthActivity.ClientID%>').value;
                CallActivity(tbname)
            }
            else if (TabPanelId == "tpReports") {
                document.getElementById('<%=hfYouthActivity.ClientID%>').value = "tpReports";
                tbname = document.getElementById('<%=hfYouthActivity.ClientID%>').value;
                CallActivity(tbname)
            }
            else if (TabPanelId == "tpDocuments") {
                document.getElementById('<%=hfYouthActivity.ClientID%>').value = "tpDocuments";
                tbname = document.getElementById('<%=hfYouthActivity.ClientID%>').value;
                CallActivity(tbname)
            }
            else if (TabPanelId == "tpContacts") {
                document.getElementById('<%=hfYouthActivity.ClientID%>').value = "tpContacts";
                tbname = document.getElementById('<%=hfYouthActivity.ClientID%>').value;
                CallActivity(tbname)
            }
            else if (TabPanelId == "tpMessages") {
                document.getElementById('<%=hfYouthActivity.ClientID%>').value = "tpMessages";
                tbname = document.getElementById('<%=hfYouthActivity.ClientID%>').value;
                CallActivity(tbname)
            }
    /* End */
            var EnTab = sender.get_activeTabIndex();
            var tcYouthDemographics = $find('<%=tcYouthDemographics.ClientID %>');
            var YouthTab = tcYouthDemographics.get_activeTabIndex();
            var IsHealthAvailable = document.getElementById('hfIsHealthAvailable').value;
            if (IsHealthAvailable == "Yes") {
                if (EnTab == 1 && YouthTab == 5) {
                    SetReferralIfram();
                } else if (EnTab == 1 && YouthTab == 1) {
                    SetHealthTabIframe();
                }
            } else {
                if (EnTab == 1 && YouthTab == 4) {
                    SetReferralIfram();
                }
            }
            return false;
        }

        function getAge(sender, args) {
            if (sender._selectedDate < new Date()) {
                var dob = sender._selectedDate
                var diff = new Date - new Date(dob);
                var diffdays = diff / 1000 / (60 * 60 * 24);
                var age = Math.floor(diffdays / 365.25);
                document.getElementById('<%=lblAge11.ClientID %>').value = age;
                document.getElementById('<%=lblAge11.ClientID %>').innerHTML = age;
                document.getElementById('<%=hfyouthage.ClientID %>').value = age;
                var lblSmokingStatusReq = document.getElementById('<%=lblSmokingStatusReq.ClientID %>');
                if (document.getElementById('ddlSmokingStatus') != null && document.getElementById('ddlSmokingStatus') != undefined) {
            if (age < 13) {                // If Age Greater Than 13 then only ddl should enable

                        document.getElementById('ddlSmokingStatus').selectedIndex = 0;
                        document.getElementById('<%= ddlSmokingStatus.ClientID%>').disabled = true;
                        document.getElementById('<%= ddlSmokingStatus.ClientID%>').className = 'mdropdownlist_black_Resize youthState disabled';
                        lblSmokingStatusReq.style.display = 'none';
                    }
                    else {
                        document.getElementById('<%= ddlSmokingStatus.ClientID%>').className = 'mdropdownlist_black_Resize youthState';
                        document.getElementById('<%= ddlSmokingStatus.ClientID%>').disabled = false;
                        lblSmokingStatusReq.style.display = '';
                    }
                }
            }
            else {
                fnNotify("You cannot select future date!");
                sender._selectedDate = new Date();
            }
        }

        function checkDeletion(message) {
            var answer = confirm(message);
            if (answer == true) {
                return true;
            }
            else {
                return false;
            }
        }

        function printSpecial1(GridName) {
            if (document.getElementById != null) {
                var html = '<HTML>\n<HEAD>\n';
                var sty = '<link href="Styles/Site.css" rel="stylesheet" type="text/css">';
                html += sty;
                html += '\n</HEAD>\n<BODY>\n';
                var printReadyElem = document.getElementById("printReady");
                if (printReadyElem != null) {
                    html += printReadyElem.innerHTML;
                    html += "</div>";
                }
                else {
                    fnNotify("Could not find the printReady section in the HTML");
                    return;
                }
                html += '\n</BODY>\n</HTML>';
                var printWin = window.open("", "printSpecial");
                printWin.document.open();
                printWin.document.write(html);
                printWin.document.close();
                if (gAutoPrint)
                    printWin.print();
            }
            else {
                fnNotify("Sorry, the print ready feature is only available in modern browsers!");
            }
        }

        function ValidatePrintOptions() {
            var FirstOption = document.getElementById("<%=cbPrint1.ClientID %>");
            var SecondOption = document.getElementById("<%=cbPrint2.ClientID %>");
            var ThirdOption = document.getElementById("<%=cbPrint3.ClientID %>");
            var hdnPOCId = document.getElementById('<%=hdnPOCId.ClientID %>').value;
            var hdnYouthId = document.getElementById('<%=hdnYouthId.ClientID %>').value;
            var l = ((screen.width - 750) / 2);
            var t = ((screen.height - 550) / 2);

            if (FirstOption.checked) {
                window.open('ViewPOCReport.aspx?YId=' + hdnYouthId + '&PId=' + hdnPOCId + '&Rname=P&RptType=pdf', '_blank', 'toolbar=0,location=0,menubar=0,resizable=0,width=750,height=550,scrollbars=1,left=' + l + ',top=' + t);
            }
            else if (SecondOption.checked) {
                window.open('ViewPOCReport.aspx?YId=' + hdnYouthId + '&PId=' + hdnPOCId + '&Rname=A&RptType=pdf', '_blank', 'toolbar=0,location=0,menubar=0,resizable=0,width=750,height=550,scrollbars=1,left=' + l + ',top=' + t);
            }
            else if (ThirdOption.checked) {
                window.open('ViewPOCReport.aspx?YId=' + hdnYouthId + '&PId=' + hdnPOCId + '&Rname=F&RptType=pdf', '_blank', 'toolbar=0,location=0,menubar=0,resizable=0,width=750,height=550,scrollbars=1,left=' + l + ',top=' + t);
            }
            else {
                fnNotify("Please select an option to print!");
            }
            return false;
        }

        function SelectFirstOption() {
            var FirstOption = document.getElementById("<%=cbPrint1.ClientID %>");
            var SecondOption = document.getElementById("<%=cbPrint2.ClientID %>");
            var ThirdOption = document.getElementById("<%=cbPrint3.ClientID %>");
            if (FirstOption) {
                FirstOption.checked = true;
                SecondOption.checked = false;
                ThirdOption.checked = false;
            }
        }
        function POCNoAttachment() {
            var trPOCNoPermission = document.getElementById("<%=trPOCNoPermission.ClientID %>");
            $find('mpePOCEmail').hide();
            $find('POCComposemail').show();
            if (trPOCNoPermission != null) {
                trPOCNoPermission.style.display = "none";
            }
            return false;
        }
        function CheckTeamMeetingDate() {
            var Date_Time_Ok = true;
            var meetingdate = document.getElementById("<%=txtMeetingDate.ClientID %>");
            var Time = document.getElementById("<%=ddlTime.ClientID %>");
            var AmorPm = document.getElementById("<%=ddlAMorPM.ClientID %>");
            if (meetingdate.value == "__/__/____") {
                document.getElementById("<%=txtMeetingDate.ClientID %>").focus();
                Date_Time_Ok = false;
                fnNotify("Please enter Meeting Date!");
            }
            else if (Time.selectedIndex < 1) {
                Time.focus();
                Date_Time_Ok = false;
                fnNotify("Please select Meeting Time!")
            }
            else if (AmorPm.selectedIndex < 1) {
                AmorPm.focus();
                Date_Time_Ok = false;
                fnNotify("Please select AM or PM!")
            }
            else {
                if (checkMeetingReminderPossibilityForSave()) {
                    Date_Time_Ok = true;
                }
                else {
                    Date_Time_Ok = false;
                }
            }

            var MeetingType = document.getElementById("<%=ddlMeetingType.ClientID %>").selectedIndex;
            if (MeetingType < 1 && Date_Time_Ok == true) {
                document.getElementById("<%=ddlAMorPM.ClientID %>").focus();
                Date_Time_Ok = false;
                fnNotify("Please select Meeting Type!");
            }
            var location = document.getElementById("<%=ddlTeamMeetingLocation.ClientID %>").selectedIndex;
            if (location < 1 && Date_Time_Ok == true) {
                document.getElementById("<%=ddlTeamMeetingLocation.ClientID %>").focus();
                Date_Time_Ok = false;
                fnNotify("Please select Meeting Location!")
            }


            if (Date_Time_Ok == true) {
                Date_Time_Ok = CheckInvitees();
                if (Date_Time_Ok == false) {
                    fnNotify('Please select Team Members to invite to Team Meeting!');
                }
            }
            return Date_Time_Ok;
        }
        function checkMeetingReminderPossibility() {
            var MeetingDate = document.getElementById("<%=txtMeetingDate.ClientID %>").value;
            var ddlTime = document.getElementById("<%=ddlTime.ClientID %>");
            var ddlAMorPM = document.getElementById("<%=ddlAMorPM.ClientID %>");
            var SendReminderBefore48Hrs = document.getElementById("<%=chkRemindermail.ClientID %>");
            if (MeetingDate != "__/__/____" && ddlTime.selectedIndex != 0 && ddlAMorPM.selectedIndex != 0) {
                var mDate = new Date(MeetingDate);
                var iyear = mDate.getFullYear();
                var imonth = mDate.getMonth();
                var iday = mDate.getDate();
                var Time = ddlTime.options[ddlTime.selectedIndex].text.split(':');
                var HourText = Time[0];
                var MinText = Time[1];
                var AMorPMText = ddlAMorPM.options[ddlAMorPM.selectedIndex].text;
                var todaydate = new Date();
                if (AMorPMText == "PM") {
                    HourText = parseInt(HourText) + 12;
                }
                var meetingDateTime = new Date(parseInt(iyear), parseInt(imonth), parseInt(iday), parseInt(HourText), parseInt(MinText));
                if (meetingDateTime.getTime() > todaydate.getTime()) {
                    var millisec = meetingDateTime.getTime() - todaydate.getTime();
                    var HrsDiff = millisec / 3600000;
                    if (HrsDiff < 48) {
                        SendReminderBefore48Hrs.disabled = true;
                        SendReminderBefore48Hrs.checked = false;
                        fnNotify('To enable Email Reminder option, the Team Meeting Date and Time should be more than 48 hours from the current date and time.');
                    }
                    else {
                        SendReminderBefore48Hrs.disabled = false;
                    }
                    return true;
                }
                else {
                    SendReminderBefore48Hrs.disabled = true;
                    SendReminderBefore48Hrs.checked = false;
                    fnNotify('To enable Email Reminder option, the Team Meeting Date and Time should be more than 48 hours from the current date and time.');
                    return true;
                }
            }

        }

        function checkMeetingReminderPossibilityForSave() {
            var MeetingDate = document.getElementById("<%=txtMeetingDate.ClientID %>").value;
            var ddlTime = document.getElementById("<%=ddlTime.ClientID %>");
            var ddlAMorPM = document.getElementById("<%=ddlAMorPM.ClientID %>");
            var SendReminderBefore48Hrs = document.getElementById("<%=chkRemindermail.ClientID %>");
            if (MeetingDate != "__/__/____" && ddlTime.selectedIndex != 0 && ddlAMorPM.selectedIndex != 0) {
                var mDate = new Date(MeetingDate);
                var iyear = mDate.getFullYear();
                var imonth = mDate.getMonth();
                var iday = mDate.getDate();
                var Time = ddlTime.options[ddlTime.selectedIndex].text.split(':');
                var HourText = Time[0];
                var MinText = Time[1];
                var AMorPMText = ddlAMorPM.options[ddlAMorPM.selectedIndex].text;
                var todaydate = new Date();
                if (AMorPMText == "PM") {
                    HourText = parseInt(HourText) + 12;
                }
                var meetingDateTime = new Date(parseInt(iyear), parseInt(imonth), parseInt(iday), parseInt(HourText), parseInt(MinText));
                if (meetingDateTime.getTime() > todaydate.getTime()) {
                    var millisec = meetingDateTime.getTime() - todaydate.getTime();
                    var HrsDiff = millisec / 3600000;
                    if (HrsDiff < 48) {
                        SendReminderBefore48Hrs.disabled = true;
                        SendReminderBefore48Hrs.checked = false;
                    }
                    else {
                        SendReminderBefore48Hrs.disabled = false;
                    }
                    return true;
                }
                else {
                    SendReminderBefore48Hrs.disabled = true;
                    SendReminderBefore48Hrs.checked = false;
                    // fnNotify('To enable Email Reminder option, the Team Meeting Date and Time should be more than 48 hours from the current date and time.');
                    return true;
                }
            }

        }

        function CheckEditTeamMeetingDate() {
            var Date_Time_Ok = true;
            var meetingdate = document.getElementById("<%=txtEMeetingDate.ClientID %>");
            if (meetingdate.value == "__/__/____") {
                document.getElementById("<%=txtEMeetingDate.ClientID %>").focus();
                Date_Time_Ok = false;
                fnNotify("Please enter meeting date!");
            }
            var duration = document.getElementById("<%=ddlTeamMeetingDuration.ClientID %>").selectedIndex;
            if (duration < 1 && Date_Time_Ok == true) {
                document.getElementById("<%=ddlTeamMeetingDuration.ClientID %>").focus();
                Date_Time_Ok = false;
                fnNotify("Please select Meeting Duration!")
            }
            if (Date_Time_Ok == true) {
                Date_Time_Ok = CheckAttendance();
                if (Date_Time_Ok == false) {
                    fnNotify('Please select Team Members who attended Team Meeting!');
                }
            }
            return Date_Time_Ok;
        }

        function CheckAttendance() {
            var cblTeam = document.getElementById('<%= cblstInvitedMemberEnterDataTeamMeeting.ClientID %>');
            var InviteesOK = false;
            if (cblTeam) {
                var cblTeamItemList = cblTeam.getElementsByTagName('input');
                if (cblTeamItemList) {
                    if (cblTeamItemList.length > 0) {
                        for (var i = 0; i < cblTeamItemList.length; i++) {
                            var cblTeamItem = cblTeamItemList[i];
                            if (cblTeamItem) {
                                if (cblTeamItem.type == 'checkbox') {
                                    if (cblTeamItem.checked) {
                                        InviteesOK = true;
                                    }
                                }
                            }
                        }
                    }
                }
            }
            return InviteesOK;
        }

        function CheckUpdateTeamMeetingDate() {
            var Date_Time_Ok = true;
            var meetingdate = document.getElementById("<%=txtEditTeamMeetingDate.ClientID %>");
            if (meetingdate.value == "__/__/____") {
                document.getElementById("<%=txtEditTeamMeetingDate.ClientID %>").focus();
                Date_Time_Ok = false;
                fnNotify("Please enter Meeting Date!");
            }

            var location = document.getElementById("<%=ddlEditTeamMeetingLocation.ClientID %>").selectedIndex;
            if (location < 1 && Date_Time_Ok == true) {
                document.getElementById("<%=ddlEditTeamMeetingLocation.ClientID %>").focus();
                Date_Time_Ok = false;
                fnNotify("Please select Meeting Location!")
            }

            var Time = document.getElementById("<%=ddlEditTeamMeetingTime.ClientID %>");
            if (Time.selectedIndex < 1 && Date_Time_Ok == true) {
                Time.focus();
                Date_Time_Ok = false;
                fnNotify("Please select Meeting Time!")
            }

            var AmorPm = document.getElementById("<%=ddlEditTeamMeetingAMorPM.ClientID %>");
            if (AmorPm.selectedIndex < 1 && Date_Time_Ok == true) {
                document.getElementById("<%=ddlEditTeamMeetingAMorPM.ClientID %>").focus();
                Date_Time_Ok = false;
                fnNotify("Please select AM or PM!");
            }
            if (Date_Time_Ok == true) {
                Date_Time_Ok = CheckInvitedMemberEdit();
                if (Date_Time_Ok == false) {
                    fnNotify('Please select Team Members to invite to Team Meeting!');
                }
            }
            var IsReminder = document.getElementById("<%=chkMeetingReminderEdit.ClientID %>");
            if (Date_Time_Ok == true && IsReminder.checked == true) {
                var GetTime = Time.options[Time.selectedIndex].text.split(":");
                var hr = GetTime[0];
                var min = GetTime[1];
                var duration = AmorPm.options[AmorPm.selectedIndex].text;
                Date_Time_Ok = checkMeetingDateDifference(meetingdate.value, hr, min, duration);
            }
            return Date_Time_Ok;
        }

        function checkMeetingDateDifference(MeetingDate, hr, min, Duration) {
            var lblMeetingNotify = document.getElementById('<%= lblMeetingNotify.ClientID %>');
            var lbMeetingReminderNo = document.getElementById('<%= lbMeetingReminderNo.ClientID %>');
            var lbMeetingReminderYes = document.getElementById('<%= lbMeetingReminderYes.ClientID %>');
            var hfIsReminderMailSend = document.getElementById('<%= hfIsReminderMailSend.ClientID %>');
            var IsReminder = document.getElementById("<%=chkMeetingReminderEdit.ClientID %>");
            var dateGiven = MeetingDate.split("/");
            var giventime = 0;
            if (Duration == "PM") {
                giventime = (parseInt(hr) + 12);
            }
            else {
                giventime = parseInt(hr);
            }
            var Todaydate = new Date();
            var GivenDate = new Date(parseInt(dateGiven[2]), parseInt(dateGiven[0]) - 1, parseInt(dateGiven[1]), giventime, min);
            if (Todaydate.getTime() < GivenDate.getTime()) {
                var millisec = parseInt(GivenDate.getTime()) - parseInt(Todaydate.getTime());
                var HrsDiff = millisec / 3600000;
                if (HrsDiff < parseInt(48)) {
                    lbMeetingReminderYes.style.display = "none";
                    lbMeetingReminderNo.innerHTML = "OK";
                    IsReminder.checked = false;
                    lblMeetingNotify.innerHTML = "There is insufficient time to send an updated Reminder. Please ensure all Invited Team Members are notified of changes to the Team Meeting.";
                    $find('mpeEditTeamMeetingDetails').hide();
                    $find('mpeMeetingReminderNote').show();
                    return false;
                }
                else if (hfIsReminderMailSend.value == "True") {
                    lbMeetingReminderYes.style.display = "";
                    lblMeetingNotify.innerHTML = "A Reminder has already been sent for this Meeting. Would you like to send an Updated Reminder?";
                    $find('mpeEditTeamMeetingDetails').hide();
                    $find('mpeMeetingReminderNote').show();
                    return false;
                }
                else {
                    return true;
                }
            }
            else {
                lbMeetingReminderYes.style.display = "none";
                lbMeetingReminderNo.innerHTML = "OK";
                IsReminder.checked = false;
                lblMeetingNotify.innerHTML = "To enable Email Reminder option, the Team Meeting Date and Time should be more than 48 hours from the current date and time.";
                $find('mpeEditTeamMeetingDetails').hide();
                $find('mpeMeetingReminderNote').show();
                return false;
            }
        }
        function CloseMeetingAlert() {
            $find('mpeMeetingReminderNote').hide();
            $find('mpeEditTeamMeetingDetails').show();
            return true;
        }

        function SelectAllTeamMembers() {
            var cblTeam = document.getElementById('<%= cblTeamMembers.ClientID %>');
            if (cblTeam) {
                var cblTeamItemList = cblTeam.getElementsByTagName('input');
                if (cblTeamItemList) {
                    if (cblTeamItemList.length > 0) {
                        for (var i = 0; i < cblTeamItemList.length; i++) {
                            var cblTeamItem = cblTeamItemList[i];
                            if (cblTeamItem) {
                                if (cblTeamItem.type == 'checkbox') {
                                    cblTeamItem.checked = true;
                                }
                            }
                        }
                    }
                }
            }
        }

        function ClearAllTeamMembers() {
            var cblTeam = document.getElementById('<%= cblTeamMembers.ClientID %>');
            if (cblTeam) {
                var cblTeamItemList = cblTeam.getElementsByTagName('input');
                if (cblTeamItemList) {
                    if (cblTeamItemList.length > 0) {
                        for (var i = 0; i < cblTeamItemList.length; i++) {
                            var cblTeamItem = cblTeamItemList[i];
                            if (cblTeamItem) {
                                if (cblTeamItem.type == 'checkbox') {
                                    cblTeamItem.checked = false;
                                }
                            }
                        }
                    }
                }
            }
        }

