        }
        function CheckDateSigned() {
            var selectedval = $("#ddlIsFidelitySigned").val();
            var txtDateSigned = document.getElementById('txtDateSigned');
            var lbltxtDateSignedreq = document.getElementById('lbltxtDateSignedreq');
            if (selectedval == "0" || selectedval == "") {
                txtDateSigned.disabled = true;
                lbltxtDateSignedreq.style.display = 'none';
                txtDateSigned.value = "__/__/____";
            }
            else {
                txtDateSigned.disabled = false;
                lbltxtDateSignedreq.style.display = '';
            }
        }

        function checkSignedDate(sender, args) {
            if (sender._selectedDate > new Date()) {
                fnNotify("Date Signed cannot be a future date!");
                sender._textbox.set_Value('__/__/____');
            }
        }

        function CheckDateSignedDate(DateSigned) {
            var ds_ok = false;
            if (checkValidDate(DateSigned, 'Consent for Fidelity Assessment Date')) {
                var dSigned = DateSigned.value;
                var current = new Date();
                var currentyear = current.getFullYear();
                var currentmonth = current.getMonth();
                currentmonth = currentmonth + 1;
                var currentday = current.getDate();
                var cmonth = '';
                var cday = '';
                if (String(currentmonth).length == 1) { cmonth = (new String('0') + new String(currentmonth)); }
                else { cmonth = new String(currentmonth); }
                if (String(currentday).length == 1) { cday = (new String('0') + new String(currentday)); }
                else { cday = new String(currentday); }
                var c_date = new String(new String(currentyear) + cmonth + cday);
                c_date = parseInt(c_date);
                var split_dob = dSigned.split("/");
                var e_date = new String(split_dob[2] + split_dob[0] + split_dob[1]);
                e_date = parseInt(e_date);
                if (e_date > c_date) {
                    fnNotify('Date Signed cannot be a future date!');
                    DateSigned.value = '__/__/____';
                    DateSigned.focus();
                    ds_ok = false;
                }
                else {
                    ds_ok = true;
                }
            }
            return ds_ok;
        }

        function CheckActivePOCLanguage() {
            var txtPreferredLanuage = document.getElementById('<%=txtPreferredLanguage.ClientID %>').value;
            var txtOtherPreferredlanguage = document.getElementById('<%=txtOtherPreferredlanguage.ClientID %>');
            var isValid = false;
            if (txtPreferredLanuage == "Other" || txtPreferredLanuage == "other") {
                if (txtOtherPreferredlanguage.value == '') {
                    fnNotify('Please enter a response for other language!');
                    isValid = false;
                }
                else {
                    isValid = true;
                }
            }
            else {
                isValid = true;
            }

            return isValid;
        }

        function EnableLanguageOtherTextbox() {
            var txtPreferredLanuage = document.getElementById('<%=txtPreferredLanguage.ClientID %>').value;
            var txtOtherPreferredlanguage = document.getElementById('<%=txtOtherPreferredlanguage.ClientID %>');
            var lbPreferredLanguageReq = document.getElementById('<%=lbPreferredLanguageReq.ClientID%>');
            if (txtPreferredLanuage == "Other" || txtPreferredLanuage == "other") {
                txtOtherPreferredlanguage.disabled = false;
                if (lbPreferredLanguageReq != null && lbPreferredLanguageReq != undefined) {
                    lbPreferredLanguageReq.style.display = '';
                }
            }
            else {
                txtOtherPreferredlanguage.disabled = true;
                txtOtherPreferredlanguage.value = '';
                if (lbPreferredLanguageReq != null && lbPreferredLanguageReq != undefined) {
                    lbPreferredLanguageReq.style.display = 'none';
                }
            }
        }

        function SetHealthTabIframe() {
            SetsessionforHealthactivetab('');
            var frame = document.getElementById("iframeHealth");
            var YouthID = document.getElementById('<%=hdnYouthId.ClientID %>').value
            var source = "../Youth/YouthHealth.aspx?YID=" + YouthID;
            frame.setAttribute("src", source);
        }

        function ReloadFamilyHistory() {
            PopupClose('mpeCommonPopup');
            var frame = document.getElementById("iframeHealth");
            frame.setAttribute("src", "");
            var YouthID = document.getElementById('<%=hdnYouthId.ClientID %>').value
            var source = "../Youth/YouthHealth.aspx?YID=" + YouthID;
            frame.setAttribute("src", source);
        }

        function ReloadImunization() {
            PopupClose('mpeCommonPopup');
            var frame = document.getElementById("iframeHealth");
            frame.setAttribute("src", "");
            var YouthID = document.getElementById('<%=hdnYouthId.ClientID %>').value
            var source = "../Youth/YouthHealth.aspx?YID=" + YouthID;
            frame.setAttribute("src", source);
            setPOpupWidthBasedonScreen();
            setContentPageHeight();
        }

        function CommonPopupForReport(source, height, width) {
            $find("mpeCommonPopup").hide();
            var youthName = document.getElementById('<%=lblNameDashboard.ClientID %>').innerHTML;
            var wrapper = document.getElementById("pnlCommonPopup");
            var frame = document.getElementById("ifrmCommonPopup");
            var heading = document.getElementById("lblCommonPopupHeader");
            heading.innerHTML = 'Report';
            if (wrapper != null && frame != null && heading != null) {
                $(".dynamicwidth").css("width", width + 'px');
                $(".dynamicHeight").css("height", height + 'px');
                frame.setAttribute("height", height + 'px');
                frame.setAttribute("width", width + 'px');
                frame.setAttribute("src", source + "&yName=" + youthName);
                $(".div-reports-top").removeAttr('style');
                $find("mpeCommonPopup").show();
            }
            return false;
        }

        function Closecommompopup() {
            setPOpupWidthBasedonScreen();
            setContentPageHeight();
            var heading = document.getElementById("lblCommonPopupHeader");
            heading.innerHTML = '';
            $(".div-reports-top").attr('style', 'display:none');
            return false;
        }

        function FamilyHisFrame(source, height, width) {
            $find("mpeCommonPopup").hide();
            var youthName = document.getElementById('<%=lblNameDashboard.ClientID %>').innerHTML;
            var wrapper = document.getElementById("pnlCommonPopup");
            var frame = document.getElementById("ifrmCommonPopup");
            var heading = document.getElementById("lblCommonPopupHeader");
            if (wrapper != null && frame != null && heading != null) {
                frame.setAttribute("height", height + 'px');
                frame.setAttribute("width", 100 + '%');
                $(wrapper).css("height", height + 'px');
                $(wrapper).css("width", '700px');
                frame.setAttribute("src", source + "&yName=" + youthName);
                $find("mpeCommonPopup").show();
            }
            return false;
        }

