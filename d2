        function ShowAddStatus() {
            $find("mpeStatusHistory").hide();
            $find("AddEnrollmentStatus").show();
            var txtEnrolledDate = document.getElementById('<%= txtEnrolledDate.ClientID %>');
            var ddlEnrollmentStatus = document.getElementById('<%= ddlEnrollmentStatus.ClientID %>');
            var ddlStatusReason = document.getElementById('<%= ddlStatusReason.ClientID %>');
            var ddlYouthOrganization = document.getElementById('<%= ddlYouthOrganization.ClientID %>');
            txtEnrolledDate.value == "__/__/____";
            ddlEnrollmentStatus.selectedIndex = 0;
            ddlYouthOrganization.selectedIndex = 0;
            ddlStatusReason.selectedIndex = 0;
            return false;
        }

        function getReportForYouth(source) {
            var youthId = '<%= Request.QueryString["YID"] %>';
            var lbContactReportFull = document.getElementById('<%= lbContactReportFull.ClientID %>');
            var lbContactReportShort = document.getElementById('<%= lbContactReportShort.ClientID %>');
            var lbContactReportLong = document.getElementById('<%= lbContactReportLong.ClientID %>');
            var pageURL = '', width = 800, height = 600;
            var title = '';
            switch (source) {
                case 'Youth':
                    pageURL = 'ViewSinglecaseReport.aspx?Y=' + youthId + '&r=' + source;
                    title = "Youth Report";
                    break;
                case 'FullReport':
                    if (lbContactReportFull.className != 'aspNetDisabled button disabled') {
                        pageURL = 'ViewContactProgressReport.aspx?Y=' + youthId + '&r=' + source;
                    }
                    title = "Contact/Progress Full Report";
                    break;
                case 'ShortSum':
                    if (lbContactReportShort.className != 'aspNetDisabled button disabled') {
                        pageURL = 'ViewContactProgressReport.aspx?Y=' + youthId + '&r=' + source;
                    }
                    title = "Contact/Progress Short Summary";
                    break;
                case 'FullSum':
                    if (lbContactReportLong.className != 'aspNetDisabled button disabled') {
                        pageURL = 'ViewContactProgressReport.aspx?Y=' + youthId + '&r=' + source;
                    }
                    title = "Contact/Progress Full Summary";
                    break;
                default:
                    pageURL = 'ViewContactProgressReport.aspx?Y=' + youthId + '&r=' + source;
                    break;
            }
            setTargetFrame(pageURL, width, height, title);
        }

        function setTargetFrame(targetFrameURL, width, height, title, condition) {
            if (title === undefined) {
                title = ''
            }
            if (condition === undefined) {
                condition = ''
            }
            setPopupWidthDynamicallyBasedonContent('CommonPoupWithHeader');

            if (title == '') {
                TargetFrame(targetFrameURL, height, width, title);
            }
            else {
                TargetFrame(targetFrameURL, height, width, title, 'Yes');
            }
            return false;
        }

        function OnClientAsyncFileUploadComplete(sender, args) {
            var upload = document.getElementById('<%= lbUpload.ClientID %>');
            var cancel_upload = document.getElementById('<%=lbCancelUpload.ClientID %>');
            var fileContentLength = parseInt(args.get_length())
            if (fileContentLength < 5000005) {
                if (args.get_errorMessage() == null) {
                    $('#ctl00_MainContent_tcEnrollment_tpDocuments_afuDocument_ctl02').css("background-color", "#CCCCCC");
                    if (upload != null) {
                        upload.style.display = '';
                    }
                    if (cancel_upload != null) {
                        cancel_upload.style.display = '';
                    }
                }
                else { fnNotify(args.get_errorMessage()); }
            }
            else {
                $('#ctl00_MainContent_tcEnrollment_tpDocuments_afuDocument_ctl02').css("background-color", "Red");
                if (upload != null) {
                    upload.style.display = 'none';
                }
                if (cancel_upload != null) {
                    cancel_upload.style.display = 'none';
                }
                fnNotify('Maximum file size allowed is 5MB! Please choose another file to upload!');
            }
            if ($("#lbComposeMail") != null && $("#lbComposeMail") != undefined && !$("#lbComposeMail").hasClass("aspNetDisabled")) {
                $("#lbComposeMail").removeClass("disabled");
                $("#lbComposeMail").addClass("blue");
            }
            if ($("#lbComposeMaiNpPermission") != null && $("#lbComposeMaiNpPermission") != undefined && !$("#lbComposeMaiNpPermission").hasClass("aspNetDisabled")) {
                $("#lbComposeMaiNpPermission").removeClass("disabled");
                $("#lbComposeMaiNpPermission").addClass("blue");
            }
        }

        function OnClientUploadError(sender, args) {
            if (args.get_errorMessage() != null) {
                fnNotify('Maximum file size allowed is 5MB! Please choose another file to upload!');
            }
        }

        function OnClientUploadStarted(sender, args) {
            if ($("#lbComposeMail") != null && $("#lbComposeMail") != undefined && !$("#lbComposeMail").hasClass("aspNetDisabled")) {
                $("#lbComposeMail").removeClass("blue");
                $("#lbComposeMail").addClass("disabled");
            }
            if ($("#lbComposeMaiNpPermission") != null && $("#lbComposeMaiNpPermission") != undefined && !$("#lbComposeMaiNpPermission").hasClass("aspNetDisabled")) {
                $("#lbComposeMaiNpPermission").removeClass("disabled");
                $("#lbComposeMaiNpPermission").addClass("blue");
            }
        }

        function CheckButtonEnable() {
            if (!$("#lbComposeMail").hasClass("disabled")) {
                return true;
            }
            else {
                return false;
            }
        }

        function TempMsg(msg, msg_element) {
            msg_element.innerHTML = msg;
            var func = function () { ClearWorkFlowMessages(msg_element); };
            setTimeout(func, 2000);
            msg_element.className = 'WorkFlowMessage';
        }
        function ClearWorkFlowMessages(msg_element) {
            msg_element.innerHTML = '';
            msg_element.className = '';
        }
        function highlight(el) {
            var fn = function () { toggler(el); };
            myfn = setInterval(fn, 500);
        }
        function stop() {
            clearInterval(myfn);
            document.getElementById('<%=trCrisisPlan.ClientID %>').className = '';
            document.getElementById('<%=trFamilyVisionStrengths.ClientID %>').className = '';
            document.getElementById('<%=trYouthNeeds.ClientID %>').className = '';
            document.getElementById('<%=trTeamMissionVision.ClientID %>').className = '';
        }
        function toggler(el) {
            if (el.className == '') { el.className = 'Highlight'; } else { el.className = ''; }
        }
        function TimeLine() {
            var youthId = '<%= Request.QueryString["YID"] %>';
            var youthName = document.getElementById('<%=lblNameDashboard.ClientID %>').innerHTML;
            var caseNumber = document.getElementById('<%=lblCaseNumberDashboard.ClientID %>').innerHTML;
            if (caseNumber != '') {
                window.location.href = 'Timeline.aspx?YID=' + youthId + '&YN=' + youthName + '&YC=' + caseNumber;
            }
            else {
                window.location.href = 'Timeline.aspx?YID=' + youthId + '&YN=' + youthName;
            }
        }
        function ValidateSchoolDetails() {
            var school_ok = false;
            var msg = '';
            var start_date = document.getElementById('<%=txtSchStartDate.ClientID %>');
            var IsEnroll_School = document.getElementById('<%=chkEnrollinsch.ClientID %>');
            var school_name = document.getElementById('<%=txtSchName.ClientID %>');
            var current_grade = document.getElementById('<%=ddlCurrGrade.ClientID %>');
            var general_edu = document.getElementById('<%=chkGE.ClientID %>');
            var special_edu = document.getElementById('<%=chkSE.ClientID %>');
            var school_placement = document.getElementById('<%=ddlSchPlaType.ClientID %>');
            var school_year = document.getElementById('<%=ddlSchYear.ClientID %>');

            if (start_date.value != '__/__/____' && start_date.value != '') {
                if (CheckSchoolStartDate()) {
                    if (IsEnroll_School.checked) {
                        if (school_name.value != '') {
                            if (current_grade.selectedIndex != 0) {
                                if (special_edu.checked) {
                                    if (school_placement.selectedIndex != 0) {
                                        school_ok = true;
                                    }
                                    else {
                                        school_ok = false;
                                        msg = 'School Placement Type is required!';
                                    }
                                }
                                else {
                                    school_ok = true;
                                }
                                if (school_ok) {
                                    school_ok = false;
                                    if (school_year.selectedIndex != 0) {
                                        school_ok = true;
                                    }
                                    else {
                                        school_ok = false;
                                        msg = 'School Year is required!';
                                    }
                                }
                            }
                            else {
                                school_ok = false;
                                msg = 'Current Grade is required!';
                            }
                        }
                        else {
                            school_ok = false;
                            msg = 'School Name is required!';
                        }
                    }
                    else {
                        school_ok = true;
                    }
                }
                else {
                    school_ok = false;
                    msg = 'Start Date cannot be a future date!';
                }
            }
            else {
                school_ok = false;
                msg = 'Start Date is required!';
            }
            if (!school_ok) {
                fnNotify(msg);
            }
            return school_ok;
        }

        function CheckStatusDetails() {
            var enrollment = document.getElementById('<%=ddlEnrollmentStatus.ClientID %>');
            var enrollment_status = document.getElementById('<%=ddlEnrollmentStatus.ClientID %>').selectedIndex;
            var enrollmentStatusControl = document.getElementById('<%=ddlEnrollmentStatus.ClientID %>');
            var enrollmentStatusValue = enrollmentStatusControl.options[enrollmentStatusControl.selectedIndex].value;
            var enrollment_date = document.getElementById('<%=txtEnrolledDate.ClientID %>').value;
            var status_reason = document.getElementById('<%=ddlStatusReason.ClientID %>');
            var organisation = document.getElementById('<%=ddlYouthOrganization.ClientID %>');
            var continue_check = true;

            if (continue_check == true && enrollment_date == '__/__/____') { fnNotify('Please set start date!'); continue_check = false; }
            if (continue_check == true && enrollment_status == 0) { fnNotify('Please select an enrollment status!'); continue_check = false; }
            if (continue_check == true && (enrollmentStatusValue != 'selected' && enrollmentStatusValue != 'Discharged' && enrollmentStatusValue != 'Not Enrolled' && enrollmentStatusValue != 'Pending') && organisation.selectedIndex == 0) { fnNotify('Please select an organization!'); continue_check = false; }
            if (continue_check == true && enrollment_status > 2) { if (status_reason.selectedIndex == 0) { fnNotify('Please select a reason!'); continue_check = false; } }
            return continue_check;
        }

        function ClearStatusDetails() {
            document.getElementById('<%=txtEnrolledDate.ClientID %>').value = '__/__/____';
            document.getElementById('<%=ddlEnrollmentStatus.ClientID %>').selectedIndex = 0;
            document.getElementById('<%=ddlStatusReason.ClientID %>').disabled = true;
            ShowYouthEnrollmentHistory();
            return false;
        }

        function ClearAddNonUserControls() {
            document.getElementById('<%=txtNonUserEmail.ClientID %>').value = '';
            document.getElementById('<%=txtNonUserFirstName.ClientID %>').value = '';
            document.getElementById('<%=txtNonUserLastname.ClientID %>').value = '';
        }

        function ClearAddTeamRoleControl() {
            document.getElementById('<%=txtRoleName.ClientID %>').value = '';
        }

        function ClearAddTeamRoleControlEdit() {
            document.getElementById('<%=txtEditRoleName.ClientID %>').value = '';
        }

        function CompareDate(GreaterDateMsg) {
            var date1 = document.getElementById('<%=txtServiceStart.ClientID %>');
            if (GreaterDateMsg == 'Task due date') {
                var date2 = document.getElementById('<%=txtTaskDueDate.ClientID %>');
            }
            else if (GreaterDateMsg == 'End date') {
                var date2 = document.getElementById('<%=txtServiceEnd.ClientID %>');
            }
        var lesserdate = false;

        if (date2.value != '__/__/____') {
            var date_two = new Date(date2.value);
            if (date_two != 'Invalid Date') {
                if (date1.value != '__/__/____') {
                    var date_one = new Date(date1.value);
                    if (date_one != 'Invalid Date') {
                        if ((dates.compare(date_two, date_one)) == -1) {
                            fnNotify(GreaterDateMsg + ' should not be lesser than start date!');
                            date2.value = '__/__/____';
                            date2.focus();
                        }
                    }
                }
            }
            else {
                fnNotify(GreaterDateMsg + ' is not valid!');
                date2.value = '__/__/____';
                date2.focus();
            }
        }
    }

    function CompareStartEndDate() {
        var txtServiceStart = document.getElementById('<%=txtServiceStart.ClientID %>');
            var txtServiceEnd = document.getElementById('<%=txtServiceEnd.ClientID %>');
            //var txtTaskDueDate = document.getElementById('<%=txtTaskDueDate.ClientID %>');
            var lesserdate = false;

            if (txtServiceStart.value != '__/__/____') {
                var start_date = new Date(txtServiceStart.value);
                if (start_date != 'Invalid Date') {
                    if (txtServiceEnd.value != '__/__/____') {
                        var end_date = new Date(txtServiceEnd.value);

                        if (end_date != 'Invalid Date') {
                            if ((dates.compare(end_date, start_date)) == -1) {
                                fnNotify('End date should not be lesser than start date!');
                                txtServiceEnd.value = '__/__/____';
                                txtServiceEnd.focus();
                            }
                        }
                    }
                    //                    if (txtTaskDueDate.value != '__/__/____') {
                    //                        var taskdue_date = new Date(txtTaskDueDate.value);

                    //                        if (taskdue_date != 'Invalid Date') {
                    //                            if ((dates.compare(taskdue_date, start_date)) == -1) {
                    //                                fnNotify('Task due date should not be lesser than start date!');
                    //                                txtTaskDueDate.value = '__/__/____';
                    //                                txtTaskDueDate.focus();
                    //                            }
                    //                        }
                    //                    }
                }
                else {
                    fnNotify('Start date is not valid!');
                    txtServiceStart.value = '__/__/____';
                    txtServiceStart.focus();
                }
            }
        }

        function validateaddtask() {
            var txtServiceStart = document.getElementById('<%=txtServiceStart.ClientID %>');
            if (Page_ClientValidate("Assigntask")) {
                if (txtServiceStart.value == '__/__/____') {
                    fnNotify('Start Date is required!');
                    txtServiceStart.value = '__/__/____';
                    txtServiceStart.focus();
                    return false;
                }
                else {
                    return true;
                }
            }
            else {
                return false;
            }
        }

        function CheckYouthAccomodations() {
            var hearing_impairment = document.getElementById('<%=cbxYouthHearingImpairment.ClientID %>');
            var seeing_impairment = document.getElementById('<%=cbxYouthSeeingImpairment.ClientID %>');
            var accomodations = document.getElementById('<%=txtYouthImpairment.ClientID %>');
            if ((hearing_impairment.checked && seeing_impairment.checked) || (!hearing_impairment.checked && seeing_impairment.checked) ||
            (hearing_impairment.checked && !seeing_impairment.checked)) {
                accomodations.disabled = false;
                accomodations.value = '';
            }
            else {
                accomodations.value = '';
                accomodations.disabled = true;
            }
        }

        function CheckEmail() {
            var txtNonUserEmail = document.getElementById('<%=txtNonUserEmail.ClientID %>');
            if (Page_ClientValidate('NonUserSave')) {
                if (txtNonUserEmail.value.trim() == '') {
                    $find('NoEmail').show();
                    return false;
                }
                else {
                    return true;
                }
            }
            else {
                return false;
            }
        }

        function SetFamilyTab() {
            $find('<%=tcEnrollment.ClientID%>').set_activeTabIndex(2);
        }

        function CheckFamilyInterview() {
            var gFT = document.getElementById("<%=gvFamilyTimeline.ClientID %>");
            if (gFT.rows.length >= 2) { fnWarning('Family Interview is already added to this youth!'); return false; }
            else { return true; }
        }

