        function CheckInvitees() {
            var cblTeam = document.getElementById('<%= cblTeamMembers.ClientID %>');
            var InviteesOK = false;
            if (cblTeam) {
                var cblTeamItemList = cblTeam.getElementsByTagName('input');
                if (cblTeamItemList) {
                    if (cblTeamItemList.length > 0) {
                        for (var i = 0; i < cblTeamItemList.length; i++) {
                            var cblTeamItem = cblTeamItemList[i];
                            if (cblTeamItem) {
                                if (cblTeamItem.type == 'checkbox') {
                                    if (cblTeamItem.checked) {
                                        InviteesOK = true;
                                    }
                                }
                            }
                        }
                    }
                }
            }
            return InviteesOK;
        }

        function CheckInvitedMemberEdit() {
            var cblTeam = document.getElementById('<%= cblstEditInvitedMembers.ClientID %>');
            var InviteesOK = false;
            if (cblTeam) {
                var cblTeamItemList = cblTeam.getElementsByTagName('input');
