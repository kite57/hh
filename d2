        }

        function ShowhideCommonpopupHeader(Showhide) {
            if (Showhide == 'Show') {
                $(".div-reports-top").attr('style', 'display:none');
            }
            else {
                $(".div-reports-top").removeAttr('style');
            }
            return false;
        }

        function GetPreferredLanguageLookup() {
            document.getElementById('divpnlSearchName').style.display = "block";
            document.getElementById('<%=txtPreferredLanguage.ClientID%>').value = '';
            document.getElementById('<%=txtPreferredLanguage.ClientID%>').focus();
            return false;
        }

        function showdivpnlSearchName() {
            if ($('#divpnlSearchName').css('display') == 'none') {
                $('#divpnlSearchName').removeAttr('Style');
            }
        }

        function SelectedValue() {
            var FamilyRace = '';
            var checked_checkboxes = $("[id*=cblEthinicity11] input:checked");
            checked_checkboxes.each(function () {
                var text = $(this).closest("td").find("label").html();
                FamilyRace += text + ',';
            });
            document.getElementById('<%=txtEthinicity11.ClientID%>').value = FamilyRace;
        }
        // To validate the Race Picklist  
        function CheckRace() {
            var ChkRace = document.getElementById('<%= cblEthinicity11.ClientID %>');
            var ISRaceSelect = false;
            if (ChkRace) {
                var cblRaceList = ChkRace.getElementsByTagName('input');
                if (cblRaceList) {
                    if (cblRaceList.length > 0) {
                        for (var i = 0; i < cblRaceList.length; i++) {
                            var cblRaceItem = cblRaceList[i];
                            if (cblRaceItem) {
                                if (cblRaceItem.type == 'checkbox') {
                                    if (cblRaceItem.checked) {
                                        ISRaceSelect = true;
                                    }
                                }
                            }
                        }
                    }
                }
            }
            return ISRaceSelect;
        }

        function ShowRace() {
            if ($('#divtxtEthinicity11').css('display') == 'none') {
                $('#divtxtEthinicity11').removeAttr('Style');
            }
            else {
                $('#divtxtEthinicity11').attr('Style', 'display:none');
            }

        }

        $('#DivYouthDemographic').scroll(function () {
            $('#divtxtEthinicity11').attr('Style', 'display:none');

        });

        $('#divYouthLanguage').scroll(function () {
            $('#divpnlSearchName').attr('Style', 'display:none');

        });

        function DisableOtherCheckboxIfDeclined(cbControl, ConditionText, arg) {
            var chkBoxList = document.getElementById(cbControl);
            var chkBoxCount = chkBoxList.getElementsByTagName("input");
            var label = chkBoxList.getElementsByTagName("label");
            var chkBoxCountNext = chkBoxList.getElementsByTagName("input");
            var labelNext = chkBoxList.getElementsByTagName("label");
            for (var i = 0; i < chkBoxCount.length; i++) {
                if (label[i].innerHTML == ConditionText) {
                    if (chkBoxCount[i].checked) {
                        for (var j = 0; j < chkBoxCountNext.length; j++) {
                            if (labelNext[j].innerHTML != ConditionText) {
                                //chkBoxCount[i].prop('disabled', true); 
                                chkBoxCountNext[j].checked = false;
                                chkBoxCountNext[j].disabled = true;
                            }
                        }
                    } else {
                        for (var j = 0; j < chkBoxCountNext.length; j++) {
                            chkBoxCountNext[j].disabled = false;
                        }
                    }
                }
            }
            return true;
        }

        function SaveDeclinedtospecify() {
            var chkBoxList = document.getElementById('<%= cblEthinicity11.ClientID %>');
            var chkBoxCount = chkBoxList.getElementsByTagName("input");
            var label = chkBoxList.getElementsByTagName("label");
            var chkBoxCountNext = chkBoxList.getElementsByTagName("input");
            var labelNext = chkBoxList.getElementsByTagName("label");
            for (var i = 0; i < chkBoxCount.length; i++) {
                if (label[i].innerHTML == 'Declined to specify') {
                    if (chkBoxCount[i].checked) {
                        for (var j = 0; j < chkBoxCountNext.length; j++) {
                            if (labelNext[j].innerHTML != 'Declined to specify') {
                                chkBoxCountNext[j].disabled = true;
                            }
                        }
                    } else {
                        for (var j = 0; j < chkBoxCountNext.length; j++) {
                            chkBoxCountNext[j].disabled = false;
                        }
                    }
                }
            }
            return true;
        }

        function printUserMessges(Id, User) {
            var width = 800, height = 600, title = 'Print';
            var sr = '';
            sr = '../ViewIndividualAssessmentReport.aspx?ID=' + Id + '&User=' + User + '&FT=MP';
            setPopupWidthDynamicallyBasedonContent("PrintClientReport");
            TargetFrame(sr, 500, 800, title);
            return false;
        }

        function chkIEPplace(chk) {
            var chkList = chk.parentNode.parentNode.parentNode;
            var chks = chkList.getElementsByTagName("input");
            for (var i = 0; i < chks.length; i++) {
                if (chks[i] != chk && chk.checked) {
                    chks[i].checked = false;
                }
            }
        }

        function PopUpClose(ModelPopup) {
            $find(ModelPopup).hide();
            return false;
        }

